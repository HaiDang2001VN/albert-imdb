rganization1/> , <location1/> cho bi\\u1ebft , tr\\u00ean \\u0111\\u1ecba_b\\u00e0n v\\u1eeba x\\u1ea3y ra v\\u1ee5 \\u00e1n_m\\u1ea1ng nghi\\u00eam_tr\\u1ecdng khi\\u1ebfn 2 v\\u1ee3_ch\\u1ed3ng tr\\u1ebb t\\u1eed_vong .\", \"<location1/> <sep> <person1/> <sep> T\\u1ea1i l\\u1ec5 k\\u00fd_k\\u1ebft , \\u00f4ng Nguy\\u1ec5n_L\\u00ea_Qu\\u1ed1c_Anh , T\\u1ed5ng_Gi\\u00e1m_\\u0110\\u1ed1c c\\u1ee7a Techcombank , chia_s\\u1ebb : \\u201c M\\u1ed1i quan_h\\u1ec7 h\\u1ee3p_t\\u00e1c n\\u00e0y l\\u00e0 m\\u1ed9t ph\\u1ea7n trong chi\\u1ebfn_l\\u01b0\\u1ee3c t\\u1ed5ng_th\\u1ec3 c\\u1ee7a ch\\u00fang_t\\u00f4i \\u0111\\u1ec3 cung_c\\u1ea5p nh\\u1eefng gi\\u00e0i ph\\u00e1p t\\u00e0i_ch\\u00ednh t\\u1ed1t nh\\u1ea5t v\\u00e0 to\\u00e0n_di\\u1ec7n nh\\u1ea5t nh\\u1eb1m \\u0111\\u00e1p_\\u1ee9ng nh\\u1eefng nhu_c\\u1ea7u ng\\u00e0y_c\\u00e0ng t\\u0103ng l\\u00ean c\\u1ee7a kh\\u00e1ch_h\\u00e0ng Vi\\u1ec7t_Nam \\u0111\\u1ed1i_v\\u1edbi c\\u00e1c d\\u1ecbch_v\\u1ee5 t\\u00e0i_ch\\u00ednh . \\u201d C\\u00f3_m\\u1eb7t t\\u1ea1i l\\u1ec5 k\\u00ed_k\\u1ebft , \\u00f4ng <person1/> , T\\u1ed5ng_Gi\\u00e1m_\\u0110\\u1ed1c ph\\u1ee5_tr\\u00e1ch H\\u1ed3ng_K\\u00f4ng , Trung_Qu\\u1ed1c , Phillipines , Th\\u00e1i_Lan , Campuchia v\\u00e0 <location1/> nh\\u1eadn_x\\u00e9t : \\u201c Vi\\u1ec7t_Nam l\\u00e0 m\\u1ed9t th\\u1ecb_tr\\u01b0\\u1eddng t\\u1ea3ng tr\\u01b0\\u1edfng chi\\u1ebfn_l\\u01b0\\u1ee3c c\\u1ee7a Manulife_Ch\\u00e2u_\\u00c1 , v\\u00e0 quan_h\\u1ec7 h\\u1ee3p_t\\u00e1c \\u0111\\u1ed9c_quy\\u1ec1n d\\u00e0i_h\\u1ea1n n\\u00e0y v\\u1edbi Techcombank s\\u1ebd gi\\u00fap ch\\u00fang_t\\u00f4i ph\\u1ee5c_v\\u1ee5 t\\u1ed1t h\\u01a1n nhu_c\\u1ea7u an_to\\u00e0n t\\u00e0i_ch\\u00ednh \\u0111ang thay_\\u0111\\u1ed5i nhanh_ch\\u00f3ng c\\u1ee7a kh\\u00e1ch_h\\u00e0ng t\\u1ea1i Vi\\u1ec7t_Nam . \\u201d \\u00d4ng Paul_Nguy\\u1ec5n , T\\u1ed5ng_Gi\\u00e1m_\\u0110\\u1ed1c c\\u1ee7a Manulife Vi\\u1ec7t_Nam , c\\u0169ng \\u0111\\u1ec1_cao m\\u1ed1i quan_h\\u1ec7 h\\u1ee3p_t\\u00e1c n\\u00e