x\\u00f3m Ph\\u00fa_Trung , <location> x\\u00e3 K\\u1ef3_Ph\\u00fa </location> , huy\\u1ec7n K\\u1ef3_Anh , <location> H\\u00e0_T\\u0129nh </location> ) c\\u0169ng t\\u1ef1_\\u00fd t\\u0103ng_gi\\u00e1 b\\u00e1n x\\u0103ng cao h\\u01a1n m\\u1ee9c gi\\u00e1 \\u0111\\u01b0\\u1ee3c ni\\u00eam_y\\u1ebft 900 \\u0111 / l\\u00edt \\u0111\\u00e3 b\\u1ecb \\u0110\\u1ed9i Qu\\u1ea3n_l\\u00fd th\\u1ecb_tr\\u01b0\\u1eddng s\\u1ed1 4 x\\u1eed_ph\\u1ea1t h\\u00e0nh_ch\\u00ednh 40 tri\\u1ec7u \\u0111\\u1ed3ng v\\u00e0 \\u0111\\u00ecnh_ch\\u1ec9 ho\\u1ea1t_\\u0111\\u1ed9ng v\\u00ec ch\\u01b0a c\\u00f3 gi\\u1ea5y_ph\\u00e9p kinh_doanh .\", \"Trong ph\\u1ea7n b\\u00e0o_ch\\u1eefa cho b\\u1ecb_c\\u00e1o Nguy\\u1ec5n_Xu\\u00e2n_S\\u01a1n , lu\\u1eadt_s\\u01b0 Nguy\\u1ec5n_Th\\u1ecb_Minh_Ph\\u01b0\\u01a1ng cho bi\\u1ebft , m\\u1ed9t_s\\u1ed1 t\\u00ecnh_ti\\u1ebft m\\u1edbi li\\u00ean_quan \\u0111\\u1ebfn h\\u00e0nh_vi c\\u1ee7a Nguy\\u1ec5n_Xu\\u00e2n_S\\u01a1n v\\u1eeba x\\u1ea3y ra l\\u00e0 vi\\u1ec7c kh\\u1edfi_t\\u1ed1 v\\u1ee5 \\u00e1n l\\u1ea1m_d\\u1ee5ng ch\\u1ee9c_v\\u1ee5 quy\\u1ec1n_h\\u1ea1n chi\\u1ebfm_\\u0111o\\u1ea1t t\\u00e0i_s\\u1ea3n x\\u1ea3y ra t\\u1ea1i <organization> Nga Vietsovpetro </organization> <organization> ( VSP </organization> ) , C\\u00f4ng_ty TNHH MTV L\\u1ecdc ho\\u00e1_d\\u1ea7u B\\u00ecnh_S\\u01a1n ( BSR ) , T\\u1ed5ng_c\\u00f4ng_ty Th\\u0103m_d\\u00f2 khai_th\\u00e1c D\\u1ea7u_kh\\u00ed ( PVEP ) .\", \"Danh_t\\u00ednh nghi can g\\u00e2y ra v\\u1ee5 \\u00e1n c\\u01b0\\u1edbp c\\u1ee7a , s\\u00e1t_h\\u1ea1i b\\u00e0 Nguy\\u1ec5n_Th\\u1ecb_H . , Ph\\u00f3 ch\\u1ee7_nhi\\u1ec7m HTX th\\u1ecb_tr\\u1ea5n Lim , Ti\\u00ean_Du ( B\\u1eafc_Ninh ) l\\u00e0\\u00a0Nguy\\u1ec5n Th\\u1ecb_Th\\u1ea3o ( SN 1975 , tr\\u00fa t\\u1ea1i x\\u00f3m \\u0110\\u00f4ng_B\\u00ecnh , th\\u00f4n Du\\u1ec7_\\u0110\\u00f4ng , <location> th\\u1ecb_tr\\u1ea5n Lim </location> , huy\\u1ec7n Ti\\u00ean_Du , <location> t\\u1ec9nh B\\u1eafc_Ninh </location> ) .\", \"\\u00d4ng <person> V\\u00f5_Vi\\u1ebft_Linh </person> - \\u0110\\u1ed9i_tr\\u01b0\\u1edfng <organization> \\u0110\\u1ed9i Qu\\u1ea3n_l\\u00fd th\\u1ecb_tr\\u01b0\\u1eddng s\\u1ed1 7 </organization> ( Chi_c\\u1ee5c Qu\\u1ea3n_l\\u00fd th\\u1ecb_tr\\u01b0\\u1eddng t\\u1ec9nh H\\u00e0_T\\u0129nh ) - cho bi\\u1ebft , \\u0111\\u01a1n_v\\u1ecb n\\u00e0y \\u0111\\u00e3 l\\u1eadp bi\\u00ean_b\\u1ea3n , x\\u1eed_ph\\u1ea1t c\\u00e2y_x\\u0103ng V\\u0103n_Ph\\u01b0\\u01a1ng ( thu\\u1ed9c Doanh_nghi\\u1ec7p t\\u01b0_nh\\u00e2n th\\u01b0\\u01a1ng_m\\u1ea1i v\\u00e0 d\\u1ecbch_v\\u1ee5 V\\u0103n_Ph\\u01b0\\u01a1ng , \\u1edf x\\u00e3 K\\u1ef3_Ninh , th\\u1ecb_x\\u00e3 K\\u1ef3_Anh , H\\u00e0_T\\u0129nh ) 40 tri\\u1ec7u \\u0111\\u1ed3ng v\\u00ec n\\u00e2ng_gi\\u00e1 b\\u00e1n cao h\\u01a1n th\\u1ecb_tr\\u01b0\\u1eddng .\", \"M\\u1ed9t \\u0111i\\u1ec1u \\u0111\\u00e1ng m\\u1eebng v\\u1edbi Chelsea l\\u00e0 h\\u1ecd \\u0111\\u00e3 c\\u00f3 nhi\\u1ec1u h\\u01a1n m\\u1ed9t trung_phong bi\\u1ebft ghi_b\\u00e0n khi Zappacosta v\\u00e0 <person> Batshuayi </person> d\\u1ef1_ph\\u00f2ng c\\u1ef1c \\u1ed5n cho <person> Morata </person> .\", \"Hai l\\u1ea7n trong n\\u0103m \\u0111\\u00f3 , <organization> EU </organization> \\u0111\\u00e3 \\u0111\\u1ed1i_m\\u1eb7t v\\u1edbi \\\" nh\\u1eefng cu\\u1ed9c kh\\u1ee7ng_ho\\u1ea3ng hi\\u1ec7n_h\\u1eefu \\\" m\\u00e0 b\\u00e0 <person> Merkel </person> \\u0111\\u00e3 d\\u1eabn_d\\u1eaft \\u0111\\u1ec3 v\\u01b0\\u1ee3t qua .\", \"\\u00d4ng V\\u00f5_Vi\\u1ebft_Linh - \\u0110\\u1ed9i_tr\\u01b0\\u1edfng \\u0110\\u1ed9i Qu\\u1ea3n_l\\u00fd th\\u1ecb_tr\\u01b0\\u1eddng s\\u1ed1 7 ( Chi_c\\u1ee5c Qu\\u1ea3n_l\\u00fd th\\u1ecb_tr\\u01b0\\u1eddng t\\u1ec9nh H\\u00e0_T\\u0129nh ) - cho bi\\u1ebft , \\u0111\\u01a1n_v\\u1ecb n\\u00e0y \\u0111\\u00e3 l\\u1eadp bi\\u00ean_b\\u1ea3n , x\\u1eed_ph\\u1ea1t c\\u00e2y_x\\u0103ng V\\u0103n_Ph\\u01b0\\u01a1ng ( thu\\u1ed9c Doanh_nghi\\u1ec7p t\\u01b0_nh\\u00e2n th\\u01b0\\u01a1ng_m\\u1ea1i v\\u00e0 d\\u1ecbch_v\\u1ee5 V\\u0103n_Ph\\u01b0\\u01a1ng , \\u1edf <location> x\\u00e3 K\\u1ef3_Ninh </location> , <location> th\\u1ecb_x\\u00e3 K\\u1ef3_Anh </location> , H\\u00e0_T\\u0129nh ) 40 tri\\u1ec7u \\u0111\\u1ed3ng v\\u00ec n\\u00e2ng_gi\\u00e1 b\\u00e1n cao h\\u01a1n th\\u1ecb_tr\\u01b0\\u1eddng .\", \"VKS \\u0111\\u1ed9t_ng\\u1ed9t \\u0111\\u1ec1_ngh\\u1ecb mi\\u1ec5n h\\u00ecnh_ph\\u1ea1t \\u0111\\u1ed1i_v\\u1edbi c\\u00e1c b\\u1ecb_c\\u00e1o <person> Nguy\\u1ec5n_Vi\\u1ec7t_H\\u00e0 </person> ( nguy\\u00ean G\\u0110 <organization> Chi_nh\\u00e1nh Th\\u00e1i_B\\u00ecnh </organization> ) , Nguy\\u1ec5n_Phan_Trung_Ki\\u00ean ( nguy\\u00ean G\\u0110 Ph\\u00f2ng giao_d\\u1ecbch \\u0110\\u00f4ng_\\u0110\\u00f4 ) , Nguy\\u1ec5n_Th\\u1ecb_Loan ( nguy\\u00ean G\\u0110 Ph\\u00f2ng giao_d\\u1ecbch Trung_Y\\u00ean ) v\\u00e0 Tr\\u1ea7n_Anh_Thi\\u1ebft ( nguy\\u00ean G\\u0110 Chi_nh\\u00e1nh H\\u00e0_N\\u1ed9i ) .\", \"B\\u1ecb_c\\u00e1o <person> H\\u00e0_V\\u0103n_Th\\u1eafm </person> , nguy\\u00ean Ch\\u1ee7_t\\u1ecbch <organization> OceanBank </organization> tr\\u01b0\\u1edbc \\u0111\\u00f3 c\\u0169ng m\\u1ed9t_m\\u1ef1c kh\\u1eb3ng_\\u0111\\u1ecbnh 1.576 t\\u1ef7 \\u0111\\u1ed3ng b\\u1ecb quy_k\\u1ebft trong c\\u00e1o_tr\\u1ea1ng kh\\u00f4ng ph\\u1ea3i l\\u00e0 thi\\u1ec7t_h\\u1ea1i m\\u00e0 \\u0111\\u00f3 l\\u00e0 kho\\u1ea3n chi l\\u00e3i ngo\\u00e0i \\u0111\\u1ec3 huy_\\u0111\\u1ed9ng v\\u1ed1n , t\\u1eeb \\u0111\\u00f3 thu l\\u1ee3i_nhu\\u1eadn v\\u1ec1 cho ng\\u00e2n_h\\u00e0ng : \\u201c T\\u00f4i l\\u00e0 c\\u1ed5_\\u0111\\u00f4ng l\\u1edbn nh\\u1ea5t c\\u1ee7a OceanBank .\", \"Ng\\u01b0\\u1eddi_m\\u1eabu Brazil s\\u1edf_h\\u1eefu th\\u00e2n_h\\u00ecnh xinh nh\\u01b0 ng\\u1ecdc Fabiana_Semprebom sinh ng\\u00e0y 26/05/1984 t\\u1ea1i <location> Londrina </location> , Parana , <location> Brazil </location> .\", \"<person> David Nguy\\u1ec5n_V\\u0169 </person> - Ch\\u1ee7_T\\u1ecbch <organization> Hi\\u1ec7p_H\\u1ed9i ng\\u01b0\\u1eddi Vi\\u1ec7t_Nam t\\u1ea1i Singapore </organization> \\u0111\\u00e3 c\\u00f3 nh\\u1eefng chia_s\\u1ebb h\\u1eefu_\\u00edch v\\u1ec1 m\\u00f4i_tr\\u01b0\\u1eddng \\u0111\\u1ea7u_t\\u01b0 t\\u1ea1i Singapore cho c\\u00e1c doanh_nghi\\u1ec7p v\\u00e0 nh\\u00e0_\\u0111\\u1ea7u_t\\u01b0 Vi\\u1ec7t quan_t\\u00e2m .\", \"Qua \\u0111\\u1ea5u_tranh , C\\u00f4ng_an huy\\u1ec7n Anh_S\\u01a1n ti\\u1ebfn_h\\u00e0nh b\\u1eaft kh\\u1ea9n_c\\u1ea5p ch\\u1ee7_qu\\u1ea3n_l\\u00fd nh\\u00e0_ngh\\u1ec9 l\\u00e0 <person> Nguy\\u1ec5n_Th\\u1ecb_K\\u1ebf </person> ( SN 1968 ) tr\\u00fa t\\u1ea1i th\\u00f4n Khe_Cho\\u0103ng , x\\u00e3 Ch\\u00e2u_Kh\\u00ea , <location> huy\\u1ec7n Con_Cu\\u00f4ng </location> v\\u1ec1 h\\u00e0nh_vi ch\\u1ee9a m\\u1ea1i_d\\u00e2m .\", \"Do_\\u0111\\u00f3 , h\\u1ed3 n\\u01b0\\u1edbc_m\\u1eb7n n\\u00e0y c\\u00f3 bi\\u1ec7t_danh l\\u00e0 <location> Bi\\u1ec3n Ch\\u1ebft </location> c\\u1ee7a <location> Trung_Qu\\u1ed1c </location> .\", \"Ti\\u00eau_hu\\u1ef7 h\\u01a1n 10 t\\u1ea5n h\\u00e0u gi\\u1ed1ng v\\u00e0 h\\u00e0u nguy\\u00ean con nh\\u1eadp l\\u1eadu ND\\u0110T \\u2013 Ng\\u00e0y 22-9 , <organization> \\u0110\\u1ed3n Bi\\u00ean_ph\\u00f2ng Tr\\u00e0 C\\u1ed5 </organization> , <location> M\\u00f3ng_C\\u00e1i </location> ( Qu\\u1ea3ng_Ninh ) \\u0111\\u00e3 ph\\u1ed1i_h\\u1ee3p v\\u1edbi c\\u00e1c l\\u1ef1c_l\\u01b0\\u1ee3ng ch\\u1ee9c_n\\u0103ng ti\\u00eau_hu\\u1ef7 9,6 t\\u1ea5n h\\u00e0u gi\\u1ed1ng ( t\\u01b0\\u01a1ng_\\u0111\\u01b0\\u01a1ng 16 ngh\\u00ecn d\\u00e2y h\\u00e0u gi\\u1ed1ng ) v\\u00e0 480 kg h\\u00e0u nguy\\u00ean con nh\\u1eadp l\\u1eadu .\", \"Kh\\u00e1m_ph\\u00e1 v\\u1ebb \\u0111\\u1eb9p ti\\u1ec1m_\\u1ea9n c\\u1ee7a v\\u1ecbnh \\u0110\\u00e0_N\\u1eb5ng <location> V\\u1ecbnh \\u0110\\u00e0_N\\u1eb5ng </location> \\u0111\\u01b0\\u1ee3c \\u0111\\u00e1nh_gi\\u00e1 l\\u00e0 m\\u1ed9t \\u0111\\u1ecba_danh ch\\u01b0a \\u0111\\u01b0\\u1ee3c khai_th\\u00e1c h\\u1ebft ti\\u1ec1m_n\\u0103ng du_l\\u1ecbch c\\u1ee7a \\\" th\\u00e0nh_ph\\u1ed1 \\u0111\\u00e1ng s\\u1ed1ng nh\\u1ea5t <location> Vi\\u1ec7t_Nam </location> \\\" .\", \"\\u0110\\u1ea1i_di\\u1ec7n VKS c\\u0169ng \\u0111\\u1ec1_ngh\\u1ecb \\u00e1p_d\\u1ee5ng th\\u00eam t\\u00ecnh_ti\\u1ebft gi\\u1ea3m nh\\u1eb9 cho c\\u00e1c b\\u1ecb_c\\u00e1o Ph\\u1ea1m_Ho\\u00e0ng_Giang ( c\\u1ef1u TG\\u0110 BSC ) , <person> L\\u00ea_Th\\u1ecb_Thu_Thu\\u1ef7 </person> ( c\\u1ef1u Ph\\u00f3 TG\\u0110 <organization> Oceanbank </organization> ) , V\\u0169_Th\\u1ecb_Thu\\u1ef3_D\\u01b0\\u01a1ng ( nguy\\u00ean Tr\\u01b0\\u1edfng ban k\\u1ebf_to\\u00e1n ) , H\\u1ee9a_Th\\u1ecb_Ph\\u1ea5n ( nguy\\u00ean Ch\\u1ee7_t\\u1ecbch H\\u0110QT c\\u00f4ng_ty Ph\\u00fa_M\\u1ef9 ) \\u00c1p_d\\u1ee5ng th\\u00eam h\\u00ecnh_ph\\u1ea1t l\\u00e0 \\u00e1n_treo \\u0111\\u1ed1i_v\\u1edbi Ph\\u1ea1m_H\\u1ed3ng_T\\u1ee9 ( nguy\\u00ean Ch\\u1ee7_t\\u1ecbch H\\u0110QT c\\u00f4ng_ty BSC ) , \\u0111\\u1ec1_ngh\\u1ecb x\\u1eed_ph\\u1ea1t b\\u1ecb_c\\u00e1o T\\u1ee9 m\\u1ee9c \\u00e1n_t\\u1eeb 30-36 th\\u00e1ng t\\u00f9_treo .\", \"Qu\\u00ea ngh\\u00e8o r\\u00fang_\\u0111\\u1ed9ng khi g\\u1ea7n 80 n\\u1ea1n_nh\\u00e2n \\u0111\\u00e2m_\\u0111\\u01a1n t\\u1ed1 ch\\u1ee7 h\\u1ee5i l\\u1eeba_\\u0111\\u1ea3o G\\u1ea7n 80 ng\\u01b0\\u1eddi \\u0111\\u1ed3ng k\\u00fd \\u0111\\u01a1n g\\u1eedi b\\u00e1o_ch\\u00ed c\\u00f9ng c\\u01a1_quan_ch\\u1ee9c_n\\u0103ng <location> huy\\u1ec7n N\\u00f4ng_S\\u01a1n </location> , <location> t\\u1ec9nh Qu\\u1ea3ng_Nam </location> ph\\u1ea3n_\\u00e1nh vi\\u1ec7c b\\u1ecb ch\\u1ee7 h\\u1ee5i qu\\u1ef5t ti\\u1ec1n .\", \"B\\u1ecb_c\\u00e1o Long t\\u1ea1i phi\\u00ean_to\\u00e0 ( \\u1ea2nh : zing.vn ) Zing.vn \\u0111\\u01b0a tin , ng\\u00e0y 22/9 , TAND huy\\u1ec7n Long_H\\u1ed3 ( V\\u0129nh_Long ) m\\u1edf phi\\u00ean_to\\u00e0 x\\u00e9t_x\\u1eed b\\u1ecb_c\\u00e1o <person> D\\u01b0\\u01a1ng_Quang_Long </person> ( 51 tu\\u1ed5i , ng\\u1ee5 x\\u00e3 T\\u00e2n_Long , <location> huy\\u1ec7n Mang_Th\\u00edt </location> ) v\\u1ec1 h\\u00e0nh_vi C\\u1ed1_\\u00fd g\\u00e2y th\\u01b0\\u01a1ng_t\\u00edch .\", \"Tin t\\u1eeb H\\u1ea1t_Ki\\u1ec3m l\\u00e2m V\\u01b0\\u1eddn_qu\\u1ed1c_gia ( VQG ) Phong_Nha \\u2013 K\\u1ebb_B\\u00e0ng , t\\u1ec9nh Qu\\u1ea3ng_B\\u00ecnh , ng\\u00e0y 22/9 cho bi\\u1ebft , \\u0111\\u01a1n_v\\u1ecb n\\u00e0y \\u0111ang xem_x\\u00e9t vi\\u1ec7c x\\u1eed_l\\u00fd tr\\u00e1ch_nhi\\u1ec7m h\\u00ecnh_s\\u1ef1 \\u0111\\u1ed1i_v\\u1edbi 9 ng\\u01b0\\u1eddi_d\\u00e2n c\\u00f9ng tr\\u00fa t\\u1ea1i <location> x\\u00e3 H\\u01b0ng_Tr\\u1ea1ch </location> , huy\\u1ec7n B\\u1ed1_Tr\\u1ea1ch <location> ( Qu\\u1ea3ng_B\\u00ecnh </location> ) .\", \"Tr\\u01b0\\u1edbc \\u0111\\u00f3 , ng\\u00e0y 14/9 \\u0111\\u00e3 c\\u0169ng \\u0111\\u00e3 x\\u1ea3y ra hai v\\u1ee5 n\\u1ed5 bom t\\u01b0\\u01a1ng_t\\u1ef1 nh\\u1eb1m v\\u00e0o xe tu\\u1ea7n_tra c\\u1ee7a qu\\u00e2n_\\u0111\\u1ed9i Th\\u00e1i_Lan \\u1edf <location> t\\u1ec9nh Yala </location> , <location> mi\\u1ec1n Nam </location> Th\\u00e1i_Lan , l\\u00e0m m\\u1ed9t binh_s\\u1ef9 thi\\u1ec7t_m\\u1ea1ng v\\u00e0 20 ng\\u01b0\\u1eddi kh\\u00e1c b\\u1ecb_th\\u01b0\\u01a1ng ; trong \\u0111\\u00f3 c\\u00f3 hai d\\u00e2n_th\\u01b0\\u1eddng .\", \"Mi\\u1ec5n h\\u00ecnh_ph\\u1ea1t v\\u1edbi c\\u00e1c b\\u1ecb_c\\u00e1o : Nguy\\u1ec5n_Vi\\u1ec7t_H\\u00e0 ( G\\u0110CN Th\\u00e1i_B\\u00ecnh ) , Nguy\\u1ec5nPhan_Trung_Ki\\u00ean ( G\\u0110PGD \\u0110\\u00f4ng_\\u0110\\u00f4 ) , <person> Tr\\u1ea7n_Anh_Thi\\u1ebft </person> ( GDCN <location> H\\u00e0_N\\u1ed9i </location> ) , Nguy\\u1ec5n_Th\\u1ecb_Loan ( G\\u0110PGD Trung_Y\\u00ean ) .\", \"Sau khi \\u0103n xong , B\\u1ea3o ch\\u1edf v\\u1ee3 v\\u00e0o m\\u1ed9t con h\\u1ebbm tr\\u00ean <location> \\u0111\\u01b0\\u1eddng L\\u1ea1c_Long_Qu\\u00e2n </location> <location> ( ph\\u01b0\\u1eddng 10 </location> , qu\\u1eadn T\\u00e2n_B\\u00ecnh ) th\\u00ec ph\\u00e1t_hi\\u1ec7n m\\u1ed9t c\\u0103n nh\\u00e0 v\\u1eafng ch\\u1ee7 .\", \"Th\\u1ea5y b\\u1ea1n b\\u1ecb \\u0111\\u00e1nh , <person> Thu\\u1eadn </person> c\\u0169ng tham_gia gi\\u00fap_s\\u1ee9c <person> Tr\\u01b0\\u1eddng </person> \\u0111\\u00e1nh Nh\\u1eadt_Anh .\", \"B\\u00e3o s\\u1ed1 10 l\\u00e0 c\\u01a1n b\\u00e3o m\\u1ea1nh \\u0111\\u1ed5_b\\u1ed9 v\\u00e0o khu_v\\u1ef1c <location> mi\\u1ec1n Trung </location> , tr\\u1ef1c_ti\\u1ebfp l\\u00e0 v\\u00e0o H\\u00e0_T\\u0129nh v\\u00e0 <location> Qu\\u1ea3ng_B\\u00ecnh </location> , l\\u00e0m nhi\\u1ec1u ng\\u01b0\\u1eddi ch\\u1ebft , m\\u1ea5t_t\\u00edch v\\u00e0 b\\u1ecb_th\\u01b0\\u01a1ng , g\\u00e2y thi\\u1ec7t_h\\u1ea1i v\\u1ec1 t\\u00e0i_s\\u1ea3n tr\\u00ean 11.000 t\\u1ef7 \\u0111\\u1ed3ng .\", \"Ti\\u1ec3u_h\\u1ecdc Ho\\u00e0ng_Hoa_Th\\u00e1m : H\\u00ecnh_th\\u00e0nh \\u00fd_th\\u1ee9c giao_th\\u00f4ng an_to\\u00e0n cho h\\u1ecdc_sinh Ng\\u00e0y 22/9/2017 , Tr\\u01b0\\u1eddng ti\\u1ec3u_h\\u1ecdc Ho\\u00e0ng_Hoa_Th\\u00e1m <location> ( Ph\\u01b0\\u1eddng V\\u0129nh_Ph\\u00fac </location> , <location> Qu\\u1eadn Ba_\\u0110\\u00ecnh </location> , H\\u00e0_N\\u1ed9i ) vinh_d\\u1ef1 \\u0111\\u01b0\\u1ee3c C\\u00f4ng_an TP.\", \"Danh_t\\u00ednh nghi can g\\u00e2y ra v\\u1ee5 \\u00e1n c\\u01b0\\u1edbp c\\u1ee7a , s\\u00e1t_h\\u1ea1i b\\u00e0 Nguy\\u1ec5n_Th\\u1ecb_H . , Ph\\u00f3 ch\\u1ee7_nhi\\u1ec7m HTX th\\u1ecb_tr\\u1ea5n Lim , Ti\\u00ean_Du ( B\\u1eafc_Ninh ) l\\u00e0\\u00a0Nguy\\u1ec5n Th\\u1ecb_Th\\u1ea3o ( SN 1975 , tr\\u00fa t\\u1ea1i x\\u00f3m \\u0110\\u00f4ng_B\\u00ecnh , <location> th\\u00f4n Du\\u1ec7_\\u0110\\u00f4ng </location> , th\\u1ecb_tr\\u1ea5n Lim , huy\\u1ec7n Ti\\u00ean_Du , <location> t\\u1ec9nh B\\u1eafc_Ninh </location> ) .\", \"\\u1ea2nh : baolaocai.vn Li\\u00ean_quan \\u0111\\u1ebfn v\\u1ee5 73 h\\u1ecdc_sinh nghi ng\\u1ed9_\\u0111\\u1ed9c th\\u1ef1c_ph\\u1ea9m sau b\\u1eefa \\u0103n t\\u1ed1i_ng\\u00e0y 20/9 t\\u1ea1i Tr\\u01b0\\u1eddng Ph\\u1ed5_th\\u00f4ng D\\u00e2n_t\\u1ed9c b\\u00e1n_tr\\u00fa Ti\\u1ec3u_h\\u1ecdc v\\u00e0 Trung_h\\u1ecdc_c\\u01a1_s\\u1edf H\\u1ea7u_Th\\u00e0o <location> ( Sa_Pa </location> , <location> L\\u00e0o_Cai </location> ) , ng\\u00e0y 22/9 , S\\u1edf Gi\\u00e1o_d\\u1ee5c v\\u00e0 \\u0110\\u00e0o_t\\u1ea1o t\\u1ec9nh L\\u00e0o_Cai \\u0111\\u00e3 c\\u00f3 c\\u00f4ng_v\\u0103n y\\u00eau_c\\u1ea7u c\\u00e1c tr\\u01b0\\u1eddng_h\\u1ecdc c\\u00f3 h\\u1ecdc_sinh \\u0103n , \\u1edf n\\u1ed9i_tr\\u00fa , b\\u00e1n_tr\\u00fa trong to\\u00e0n t\\u1ec9nh t\\u0103ng_c\\u01b0\\u1eddng k\\u1ef7_c\\u01b0\\u01a1ng , n\\u1ec1n_n\\u1ebfp qu\\u1ea3n_l\\u00fd h\\u1ecdc_sinh , r\\u00e0_so\\u00e1t c\\u00e1c \\u0111i\\u1ec1u_ki\\u1ec7n b\\u1ea3o_\\u0111\\u1ea3m an_to\\u00e0n , \\u0111\\u1eb7c_bi\\u1ec7t ch\\u00fa_tr\\u1ecdng \\u0111\\u1ebfn c\\u00f4ng_t\\u00e1c b\\u1ea3o_\\u0111\\u1ea3m an_to\\u00e0n v\\u1ec7_sinh th\\u1ef1c_ph\\u1ea9m trong tr\\u01b0\\u1eddng_h\\u1ecdc , tr\\u00e1nh nh\\u1eefng v\\u1ee5_vi\\u1ec7c \\u0111\\u00e1ng ti\\u1ebfc x\\u1ea3y ra .\", \"<person> Diego_Costa </person> b\\u1ecb HLV <person> Conte </person> nh\\u1eafn_tin th\\u00f4ng_b\\u00e1o \\u201c kh\\u00f4ng c\\u00f2n n\\u1eb1m trong k\\u1ebf_ho\\u1ea1ch \\u201d c\\u1ee7a \\u00f4ng v\\u00e0o \\u0111\\u1ea7u m\\u00f9a H\\u00e8 n\\u00e0y .\", \"C\\u1ee5_th\\u1ec3 , Th\\u1eafm \\u0111\\u01b0a th\\u01b0_k\\u00fd Ho\\u00e0ng_Th\\u1ecb_H\\u1ed3ng_T\\u1ee9 ( t\\u1ee9c di\\u1ec5n_vi\\u00ean <person> Qu\\u1ef3nh_T\\u1ee9 </person> ) l\\u00ean l\\u00e0m Ch\\u1ee7_t\\u1ecbch H\\u0110QT c\\u1ee7a <organization> BSC </organization> .\", \"Chi\\u1ec1u c\\u00f9ng ng\\u00e0y , l\\u1ef1c_l\\u01b0\\u1ee3ng c\\u00f4ng_an b\\u1eaft_gi\\u1eef 3 \\u0111\\u1ed1i_t\\u01b0\\u1ee3ng l\\u00e0 <person> Ph\\u1ea1m_V\\u0103n_Hi\\u1ec7p </person> , H\\u00e0_V\\u0103n_Th\\u01b0\\u01a1ng , H\\u00e0_V\\u0103n_Thu\\u1ea7n ( c\\u00f9ng tr\\u00fa t\\u1ea1i th\\u00f4n \\u0110\\u1ed3ng L\\u1ee5c , <location> x\\u00e3 Gia_Ph\\u00fa </location> ) .\", \"Chi\\u1ec1u 22-9 , tin t\\u1eeb <organization> C\\u00f4ng_an huy\\u1ec7n \\u0110\\u1ee9c_Th\\u1ecd </organization> , <location> t\\u1ec9nh H\\u00e0_T\\u0129nh </location> cho_hay tr\\u00ean \\u0111\\u1ecba_b\\u00e0n huy\\u1ec7n n\\u00e0y v\\u1eeba x\\u1ea3y ra m\\u1ed9t v\\u1ee5 tai_n\\u1ea1n giao_th\\u00f4ng \\u0111\\u01b0\\u1eddng_s\\u1eaft khi\\u1ebfn 1 ng\\u01b0\\u1eddi \\u0111\\u00e0n_\\u00f4ng t\\u1eed_vong .\", \"VKS\\u00a0c\\u0169ng \\u0111\\u1ec1_ngh\\u1ecb H\\u0110XX xem_x\\u00e9t gi\\u1ea3m nh\\u1eb9 cho c\\u00e1c b\\u1ecb_c\\u00e1o Ph\\u1ea1m_Ho\\u00e0ng_Giang , nguy\\u00ean TG\\u0110 c\\u00f4ng_ty BSC ; <person> L\\u00ea_Th\\u1ecb_Thu_Thu\\u1ef7 </person> , nguy\\u00ean Ph\\u00f3 TG\\u0110 <organization> OceanBank </organization> ; V\\u0169_Th\\u1ecb_Thu\\u1ef3_D\\u01b0\\u01a1ng , nguy\\u00ean Gi\\u00e1m_\\u0111\\u1ed1c kh\\u1ed1i K\\u1ebf_to\\u00e1n OceanBank .\", \"\\u0110\\u01b0\\u1ee3c bi\\u1ebft , c\\u00f4ng_ty <organization> L ' Oreal </organization> \\u00a0n\\u00e0y c\\u00f3 nhi\\u1ec1u th\\u01b0\\u01a1ng_hi\\u1ec7u n\\u1ed5i_ti\\u1ebfng nh\\u01b0 <organization> Lancome </organization> , Garnier v\\u00e0 La Roche-Posay .\", \"C\\u00e1c \\u0111\\u1ea3ng kh\\u00e1c nh\\u1ecf h\\u01a1n \\u1edf <location> \\u0110\\u1ee9c </location> , nh\\u01b0 \\u0111\\u1ea3ng D\\u00e2n_ch\\u1ee7 T\\u1ef1_do <organization> ( FDP </organization> ) c\\u00f3 t\\u01b0_t\\u01b0\\u1edfng t\\u1ef1_do , \\u0111\\u1ea3ng Gr\\u00fcne ( \\u0111\\u1ea3ng Xanh ) hay \\u0111\\u1ea3ng Die_Linke ( C\\u00e1nh t\\u1ea3 ) \\u0111\\u1ec1u \\u0111ang nu\\u00f4i hy_v\\u1ecdng c\\u00f3 gh\\u1ebf t\\u1ea1i qu\\u1ed1c_h\\u1ed9i li\\u00ean_bang sau cu\\u1ed9c b\\u1ea7u_c\\u1eed n\\u00e0y .\", \"Theo c\\u01a1_quan \\u0111i\\u1ec1u_tra , kho\\u1ea3ng 12h05 ng\\u00e0y 20/9 , gia_\\u0111\\u00ecnh \\u00f4ng C\\u00e1p_Tr\\u1ecdng_T\\u00ednh ( SN 1950 , tr\\u00fa t\\u1ea1i t\\u1ea1i x\\u00f3m Ki\\u1ec1u_Ch\\u00ednh , x\\u00e3 Xu\\u00e2n_Ph\\u01b0\\u01a1ng , huy\\u1ec7n Ph\\u00fa_B\\u00ecnh ) g\\u1ecdi c\\u1eeda kh\\u00f4ng th\\u1ea5y anh <person> C\\u00e1p_Tr\\u1ecdng_Th\\u00e0nh </person> ( SN 1984 ) v\\u00e0 ch\\u1ecb <person> D\\u01b0\\u01a1ng_Th\\u1ecb_Tuy\\u1ebft </person> ( SN 1986 ) tr\\u1ea3_l\\u1eddi , b\\u00e8n ti\\u1ebfn v\\u00e0o trong ph\\u00f2ng ng\\u1ee7 v\\u00e0 ph\\u00e1t_hi\\u1ec7n hai v\\u1ee3_ch\\u1ed3ng \\u0111\\u00e3 t\\u1eed_vong .\", \"Anh <person> G </person> <location> ( \\u0110\\u1ed1ng_\\u0110a </location> \\u2013 H\\u00e0_N\\u1ed9i ) chia_s\\u1ebb : \\u201c C\\u00f3 m\\u1ed9t_s\\u1ed1 sim khi chuy\\u1ec3n t\\u1eeb 11 s\\u1ed1 sang 10 s\\u1ed1 s\\u1ebd th\\u00e0nh sim s\\u1ed1 \\u0111\\u1eb9p n\\u1ebfu h\\u1ee3p v\\u1edbi \\u0111\\u1ea7u s\\u1ed1 c\\u1ee7a nh\\u00e0 m\\u1ea1ng .\", \"\\u0110i v\\u00e0o ho\\u1ea1t_\\u0111\\u1ed9ng t\\u1eeb n\\u0103m 2015 , c\\u00f4ng_ty D\\u1ec7t_Pacific_Crystal c\\u00f3 \\u0111\\u1ecba_ch\\u1ec9 t\\u1ea1i <location> Khu c\\u00f4ng_nghi\\u1ec7p Lai_Vu </location> , <location> huy\\u1ec7n Kim_Th\\u00e0nh </location> , H\\u1ea3i_D\\u01b0\\u01a1ng khi\\u1ebfn ng\\u01b0\\u1eddi_d\\u00e2n \\u0111\\u1ecba_ph\\u01b0\\u01a1ng b\\u1ee9c_x\\u00fac v\\u00ec n\\u1ea1n x\\u1ea3 n\\u01b0\\u1edbc_th\\u1ea3i g\\u00e2y \\u00f4_nhi\\u1ec5m m\\u00f4i_tr\\u01b0\\u1eddng nghi\\u00eam_tr\\u1ecdng .\", \"VKS\\u00a0c\\u0169ng \\u0111\\u1ec1_ngh\\u1ecb H\\u0110XX xem_x\\u00e9t gi\\u1ea3m nh\\u1eb9 cho c\\u00e1c b\\u1ecb_c\\u00e1o <person> Ph\\u1ea1m_Ho\\u00e0ng_Giang </person> , nguy\\u00ean TG\\u0110 <organization> c\\u00f4ng_ty BSC </organization> ; L\\u00ea_Th\\u1ecb_Thu_Thu\\u1ef7 , nguy\\u00ean Ph\\u00f3 TG\\u0110 OceanBank ; V\\u0169_Th\\u1ecb_Thu\\u1ef3_D\\u01b0\\u01a1ng , nguy\\u00ean Gi\\u00e1m_\\u0111\\u1ed1c kh\\u1ed1i K\\u1ebf_to\\u00e1n OceanBank .\", \"Nh\\u1eefng n\\u0103m \\u0111\\u1ea7u_ti\\u00ean c\\u1ee7a <person> Merkel </person> trong vai_tr\\u00f2 l\\u00e3nh_\\u0111\\u1ea1o \\u0111\\u1ea3ng <organization> CDU </organization> kh\\u00f4ng h\\u1ec1 d\\u1ec5_d\\u00e0ng .\", \"H\\u1ed3_Ch\\u00ed_Minh th\\u00f4ng_tin , theo c\\u00e1o_tr\\u1ea1ng , Long v\\u00e0 ch\\u1ecb <person> L\\u00ea_Th\\u1ecb_L\\u1ec7 </person> ( 48 tu\\u1ed5i , ng\\u1ee5 \\u1ea5p H\\u1eadu_Th\\u00e0nh , x\\u00e3 Long_An , <location> huy\\u1ec7n Long_H\\u1ed3 </location> ) c\\u00f9ng h\\u00e0nh_ngh\\u1ec1 mua_b\\u00e1n qu\\u1ea7n_\\u00e1o \\u1edf c\\u00e1c ch\\u1ee3 trong huy\\u1ec7n .\", \"\\u1ea2nh ' \\u0111\\u1ed9c ' v\\u1ec1 nh\\u00e0_l\\u00e3nh_\\u0111\\u1ea1o <location> Tri\\u1ec1u_Ti\\u00ean </location> <person> Kim Jong-un </person> ( 2 ) Nh\\u00e0_l\\u00e3nh_\\u0111\\u1ea1o Tri\\u1ec1u_Ti\\u00ean Kim Jong-un \\u0111\\u00e3 t\\u1ef1 tay tr\\u1ed3ng c\\u00e2y_xanh trong m\\u1ed9t s\\u1ef1_ki\\u1ec7n nh\\u1eb1m khuy\\u1ebfn_kh\\u00edch \\u201c tr\\u1ed3ng c\\u00e2y g\\u00e2y r\\u1eebng \\u201d \\u1edf th\\u1ee7_\\u0111\\u00f4 B\\u00ecnh_Nh\\u01b0\\u1ee1ng .\", \"Angelian_Jolie v\\u00e0 6 \\u0111\\u1ee9a con c\\u00f9ng c\\u00e1c c\\u1ed9ng_s\\u1ef1 gi\\u1edbi_thi\\u1ec7u phim t\\u1ea1i Li\\u00ean_hoan_phim Toronto_V\\u1edbi 6 \\u0111\\u1ee9a con - Maddox , 16 , <person> Pax </person> , 13 , Zahara , 12 , <person> Shiloh </person> , 11 , Vivienne and Knox , 9 - c\\u00f9ng \\u0111i , ng\\u00f4i_sao m\\u00e0n_b\\u1ea1c 42 tu\\u1ed5i n\\u00e0y \\u0111\\u00e3 xu\\u1ea5t_hi\\u1ec7n tr\\u01b0\\u1edbc c\\u00f4ng_ch\\u00fang trong bu\\u1ed5i gi\\u1edbi_thi\\u1ec7u phim \\u2018 First_They_Killed_My Father \\u2019 ( t\\u1ea1m d\\u1ecbch \\u2018 \\u0110\\u1ea7u_ti\\u00ean h\\u1ecd \\u0111\\u00e3 gi\\u1ebft cha t\\u00f4i \\u2019 ) .\", \"Theo c\\u00e1o_tr\\u1ea1ng , v\\u00e0o ng\\u00e0y 11/6 , Y \\u0102i_Adr\\u01a1ng v\\u00e0 v\\u1ee3 l\\u00e0 <person> H \\u2019 A By\\u0103 </person> ( SN 1972 , tr\\u00fa bu\\u00f4n Sut_Mdrang , x\\u00e3 C\\u01b0_Su\\u00ea , <location> huy\\u1ec7n C\\u01b0_M \\u2019 gar </location> ) \\u0111i \\u0111\\u00e1m_ma nh\\u00e0 ng\\u01b0\\u1eddi_th\\u00e2n \\u1edf c\\u00f9ng bu\\u00f4n .\", \"Trong tr\\u1eadn g\\u1eb7p Betis m\\u1edbi_\\u0111\\u00e2y , <organization> Real </organization> c\\u0169ng b\\u1ecb \\u0111\\u1ee9t chu\\u1ed7i ghi_b\\u00e0n li\\u00ean_ti\\u1ebfp trong 73 tr\\u1eadn d\\u00f9 s\\u00fat t\\u1edbi 27 l\\u1ea7n , v\\u00e0 c\\u00f3 <person> Ronaldo </person> tr\\u1edf_l\\u1ea1i .\", \"Ng\\u01b0_d\\u00e2n Qu\\u1ea3ng_Ng\\u00e3i tr\\u00ean t\\u00e0u c\\u00e1 b\\u1ecb n\\u1ea1n \\u0111\\u01b0\\u1ee3c B\\u0110BP B\\u00ecnh_\\u0110\\u1ecbnh th\\u0103m_kh\\u00e1m s\\u1ee9c_kho\\u1ebb Tr\\u01b0\\u1edbc \\u0111\\u00f3 , t\\u00e0u c\\u00e1 QNg 95183 TS do ng\\u01b0_d\\u00e2n Nguy\\u1ec5n_V\\u0103n_Trung ( SN 1964 ) \\u1edf x\\u00e3 B\\u00ecnh_Ch\\u00e1nh <location> ( huy\\u1ec7n B\\u00ecnh_S\\u01a1n </location> , <location> Qu\\u1ea3ng_Ng\\u00e3i </location> ) l\\u00e0m ch\\u1ee7 ki\\u00eam thuy\\u1ec1n_tr\\u01b0\\u1edfng , tr\\u00ean t\\u00e0u c\\u00f3 6 lao_\\u0111\\u1ed9ng , h\\u00e0nh_ngh\\u1ec1 l\\u01b0\\u1edbi r\\u00ea , xu\\u1ea5t b\\u1ebfn t\\u1ea1i tr\\u1ea1m M\\u0169i_T\\u1ea5n v\\u00e0o ng\\u00e0y 10/9 .\", \"Ng\\u00e0y 22/9 , ch\\u00fang_t\\u00f4i c\\u00f3_m\\u1eb7t t\\u1ea1i <location> ch\\u1ee3 Trung_Ph\\u01b0\\u1edbc </location> , <location> x\\u00e3 Qu\\u1ebf_Trung </location> , huy\\u1ec7n N\\u00f4ng_S\\u01a1n , t\\u1ec9nh Qu\\u1ea3ng_Nam \\u00a0nh\\u1eb1m ti\\u1ebfp_nh\\u1eadn ph\\u1ea3n_\\u00e1nh c\\u1ee7a 77 ti\\u1ec3u_th\\u01b0\\u01a1ng trong ch\\u1ee3 .\", \"Chi\\u1ec1u 22/9 , C\\u00f4ng_an qu\\u1eadn Th\\u1ee7_\\u0110\\u1ee9c ( TP. HCM ) cho bi\\u1ebft , c\\u01a1_quan n\\u00e0y \\u0111ang \\u0111i\\u1ec1u_tra , truy_t\\u00ecm nh\\u00f3m thanh_ni\\u00ean tr\\u1ed9m 8,5 t\\u1ef7 \\u0111\\u1ed3ng_ti\\u1ec1n l\\u01b0\\u01a1ng c\\u00f4ng_nh\\u00e2n t\\u1ea1i C\\u00f4ng_ty Nh\\u01a1n_Ho\\u00e0 <location> ( qu\\u1ed1c_l\\u1ed9 13 </location> , <location> ph\\u01b0\\u1eddng Hi\\u1ec7p_B\\u00ecnh_Ph\\u01b0\\u1edbc </location> ) .\", \"Nh\\u00f3m n\\u1eef b\\u1ecb_c\\u00e1o nguy\\u00ean l\\u00e0 l\\u00e3nh_\\u0111\\u1ea1o \\u1edf h\\u1ed9i_s\\u1edf <organization> OceanBank </organization> \\u0111\\u01b0\\u1ee3c VKS \\u0111\\u1ec1_ngh\\u1ecb gi\\u1ea3m m\\u1ed9t ph\\u1ea7n h\\u00ecnh_ph\\u1ea1t g\\u1ed3m : <person> V\\u0169_Th\\u1ecb_Thu\\u1ef3_D\\u01b0\\u01a1ng </person> ( c\\u1ef1u gi\\u00e1m_\\u0111\\u1ed1c kh\\u1ed1i k\\u1ebf_to\\u00e1n v\\u00e0 giao_d\\u1ecbch trong n\\u01b0\\u1edbc ) , Nguy\\u1ec5n_Th\\u1ecb_Nga ( c\\u1ef1u k\\u1ebf_to\\u00e1n_tr\\u01b0\\u1edfng ) , Nguy\\u1ec5n_Ho\\u00e0i_Nam ( c\\u1ef1u gi\\u00e1m_\\u0111\\u1ed1c kh\\u1ed1i ngu\\u1ed3n v\\u1ed1n ) , Nguy\\u1ec5n_Th\\u1ecb_Thu_Ba ( c\\u1ef1u gi\\u00e1m_\\u0111\\u1ed1c kh\\u1ed1i ng\\u00e2n_h\\u00e0ng b\\u00e1n_l\\u1ebb ) , \\u0110\\u1ed7_\\u0110\\u1ea1i_Kh\\u00f4i_Trang ( c\\u1ef1u gi\\u00e1m_\\u0111\\u1ed1c kh\\u1ed1i kh\\u00e1ch_h\\u00e0ng c\\u00e1_nh\\u00e2n ) .\", \"B\\u00e1o Infonet \\u0111\\u01b0a tin , ng\\u00e0y 22/9 , TAND huy\\u1ec7n C\\u01b0_M \\u2019 gar ( \\u0110\\u1eafk_L\\u1eafk ) \\u0111\\u00e3 m\\u1edf phi\\u00ean_to\\u00e0 x\\u00e9t_x\\u1eed s\\u01a1_th\\u1ea9m \\u0111\\u1ed1i_v\\u1edbi b\\u1ecb_c\\u00e1o <person> Y \\u0102i_Adr\\u01a1ng </person> ( SN 1985 , ng\\u1ee5 <location> bu\\u00f4n J\\u00f9 </location> , x\\u00e3 Ea_Tu , TP Bu\\u00f4n_Ma_Thu\\u1ed9t ) v\\u1ec1 t\\u1ed9i Hi\\u1ebfp_d\\u00e2m .\", \"Chi\\u1ebfc xe n\\u00e0y \\u0111\\u00e3 \\u0111\\u1ec3 l\\u1ea1i g\\u1ea7n m\\u1ed9t ng\\u00f4i nh\\u00e0 \\u1edf <location> huy\\u1ec7n Muang </location> c\\u1ee7a <location> t\\u1ec9nh Nakhon_Pathom </location> .\", \"Trong ch\\u01b0\\u01a1ng_tr\\u00ecnh ngh\\u1ec7_thu\\u1eadt \\u0111\\u1eb7c_bi\\u1ec7t n\\u00e0y , l\\u1ea7n \\u0111\\u1ea7u_ti\\u00ean 30 BTV truy\\u1ec1n_h\\u00ecnh s\\u1ebd tr\\u00ecnh_di\\u1ec5n 3 b\\u1ed9 s\\u01b0u_t\\u1eadp th\\u1eddi_trang c\\u1ee7a c\\u00e1c th\\u01b0\\u01a1ng_hi\\u1ec7u n\\u1ed5i_ti\\u1ebfng : N\\u1ebfp_N\\u1ebfp - V\\u0169_Tr\\u1ea7n_\\u0110\\u1ee9c_H\\u1ea3i , <organization> \\u0110\\u0169i_Nam_Cao </organization> - <person> Duy_Nguy\\u1ec5n_</person> v\\u00e0 SilkyVietnam .\", \"Ng\\u00e0y 22/9 , tr\\u1ea1m ch\\u0103n_nu\\u00f4i th\\u00fa_y <location> huy\\u1ec7n Di\\u1ec5n_Ch\\u00e2u </location> , <location> Ngh\\u1ec7_An </location> cho bi\\u1ebft \\u0111\\u01a1n_v\\u1ecb v\\u1eeba ti\\u1ebfp_nh\\u1eadn b\\u00e0n_giao tang_v\\u1eadt l\\u00e0 30 th\\u00f9ng c\\u00e1t t\\u00f4ng ch\\u1ee9a h\\u01a1n 2.400 con gia_c\\u1ea7m kh\\u00f4ng r\\u00f5 ngu\\u1ed3n_g\\u1ed1c .\", \"H\\u1ed3_Ch\\u00ed_Minh th\\u00f4ng_tin , theo c\\u00e1o_tr\\u1ea1ng , Long v\\u00e0 ch\\u1ecb <person> L\\u00ea_Th\\u1ecb_L\\u1ec7 </person> ( 48 tu\\u1ed5i , ng\\u1ee5 \\u1ea5p H\\u1eadu_Th\\u00e0nh , <location> x\\u00e3 Long_An </location> , huy\\u1ec7n Long_H\\u1ed3 ) c\\u00f9ng h\\u00e0nh_ngh\\u1ec1 mua_b\\u00e1n qu\\u1ea7n_\\u00e1o \\u1edf c\\u00e1c ch\\u1ee3 trong huy\\u1ec7n .\", \"Chu\\u1ed7i li\\u00ean_k\\u1ebft <organization> De_Heus </organization> bao_g\\u1ed3m : Cty CP Bel_G\\u00e0 cung_c\\u1ea5p con_gi\\u1ed1ng ; T\\u1eadp_\\u0111o\\u00e0n De_Heus cung_c\\u1ea5p th\\u1ee9c_\\u0103n ; <organization> T\\u1eadp_\\u0111o\\u00e0n H\\u00f9ng_Nh\\u01a1n </organization> \\u0111\\u1ea1i_di\\u1ec7n c\\u00e1c trang_tr\\u1ea1i g\\u00e0 \\u0111\\u1ea1t chu\\u1ea9n ; Cty TNHH Koyu v\\u00e0 Unitek l\\u00e0 \\u0111\\u01a1n_v\\u1ecb thu_mua , gi\\u1ebft_m\\u1ed5 v\\u00e0 xu\\u1ea5t_kh\\u1ea9u .\", \"Agribank \\u0111\\u00e3 ch\\u1ee7_\\u0111\\u1ed9ng quy\\u1ebft_\\u0111\\u1ecbnh k\\u1ef7_lu\\u1eadt c\\u00e1n_b\\u1ed9 c\\u00f3 li\\u00ean_quan , c\\u1ea3nh_c\\u00e1o , chuy\\u1ec3n c\\u00f4ng_t\\u00e1c kh\\u00e1c , h\\u01b0\\u1edfng b\\u1eadc l\\u01b0\\u01a1ng th\\u1ea5p h\\u01a1n \\u0111\\u1ed1i_v\\u1edbi b\\u00e0 B\\u00f9i_Th\\u1ecb_S\\u00e1u ; c\\u1ea3nh_c\\u00e1o , chuy\\u1ec3n c\\u00f4ng_t\\u00e1c kh\\u00e1c , h\\u01b0\\u1edfng b\\u1eadc l\\u01b0\\u01a1ng th\\u1ea5p h\\u01a1n \\u0111\\u1ed1i_v\\u1edbi b\\u00e0 <person> Nguy\\u1ec5n_Th\\u1ecb_Huy\\u1ec1n </person> , c\\u00e1n_b\\u1ed9 k\\u1ebf_to\\u00e1n <organization> Agribank </organization> Ba_\\u0110\\u00ecnh ; khi\\u1ec3n_tr\\u00e1ch \\u0111\\u1ed1i_v\\u1edbi \\u00f4ng Gi\\u00e1m_\\u0111\\u1ed1c Agribank_Ba_\\u0110\\u00ecnh v\\u00e0 Gi\\u00e1m_\\u0111\\u1ed1c Agribank chi_nh\\u00e1nh t\\u1ec9nh Thanh_Ho\\u00e1 .\", \"C\\u00f9ng d\\u1ef1 c\\u00f2n c\\u00f3 ng\\u00e0i <person> Kunio_Umeda </person> , \\u0111\\u1ea1i_s\\u1ee9 <location> Nh\\u1eadt_B\\u1ea3n </location> t\\u1ea1i Vi\\u1ec7t_Nam v\\u00e0 \\u0111\\u1ea1i_di\\u1ec7n Hi\\u1ec7p_h\\u1ed9i Ph\\u00f2ng ch\\u00e1y ch\\u1eefa_ch\\u00e1y ( PCCC ) Nh\\u1eadt_B\\u1ea3n \\u2013 JCA , Nh\\u1eadt_B\\u1ea3n ( SPJD ) v\\u1ec1 d\\u1ef1_\\u00e1n vi\\u1ec7n_tr\\u1ee3 xe ch\\u1eefa_ch\\u00e1y \\u0111\\u00e3 qua s\\u1eed_d\\u1ee5ng .\", \"N\\u1eef hung_th\\u1ee7 th\\u1eeba_nh\\u1eadn \\u0111\\u00e3 d\\u00f9ng ch\\u00e0y nh\\u00f4m s\\u00e1t_h\\u1ea1i Ch\\u1ee7_nhi\\u1ec7m H\\u1ee3p_t\\u00e1c_x\\u00e3 Sau nhi\\u1ec1u ng\\u00e0y \\u0111i\\u1ec1u_tra , C\\u00f4ng_an t\\u1ec9nh B\\u1eafc_Ninh \\u0111\\u00e3 c\\u00f3 \\u0111\\u1ee7 ch\\u1ee9ng_c\\u1ee9 kh\\u1eb3ng_\\u0111\\u1ecbnh Nguy\\u1ec5n_Th\\u1ecb_Th\\u1ea3o ( SN 1975 ) , tr\\u00fa t\\u1ea1i th\\u00f4n Du\\u1ec7_\\u0110\\u00f4ng , <location> th\\u1ecb_tr\\u1ea5n Lim </location> , huy\\u1ec7n Ti\\u00ean_Du , <location> B\\u1eafc_Ninh </location> l\\u00e0 k\\u1ebb ra_tay gi\\u1ebft ch\\u1ebft b\\u00e0 Hu\\u1ea5n .\", \"\\u0110\\u00e1nh_gi\\u00e1 v\\u1ec1 vi\\u1ec7c c\\u00f3 n\\u00ean b\\u1ecf ban ph\\u1ee5_huynh trong tr\\u01b0\\u1eddng_h\\u1ecdc hay kh\\u00f4ng , TS <person> Nguy\\u1ec5n_T\\u00f9ng_L\\u00e2m </person> , Ch\\u1ee7_t\\u1ecbch <organization> H\\u1ed9i T\\u00e2m_l\\u00fd gi\\u00e1o_d\\u1ee5c H\\u00e0_N\\u1ed9i </organization> cho r\\u1eb1ng nhi\\u1ec1u ng\\u01b0\\u1eddi \\u0111ang tr\\u00e1ch nh\\u1ea7m\\u00a0ban ph\\u1ee5_huynh v\\u00ec th\\u1ef1c_t\\u1ebf ph\\u1ee5_huynh kh\\u00f4ng c\\u00f3 quy\\u1ec1n v\\u00e0 ch\\u1ee9c_n\\u0103ng \\u0111\\u1ec3 l\\u00e0m \\u0111\\u1ee7 nh\\u1eefng vi\\u1ec7c c\\u1ea7n l\\u00e0m .\", \"C\\u00e1c \\u0111\\u1ea3ng kh\\u00e1c nh\\u1ecf h\\u01a1n \\u1edf <location> \\u0110\\u1ee9c </location> , nh\\u01b0 <organization> \\u0111\\u1ea3ng D\\u00e2n_ch\\u1ee7 T\\u1ef1_do </organization> ( FDP ) c\\u00f3 t\\u01b0_t\\u01b0\\u1edfng t\\u1ef1_do , \\u0111\\u1ea3ng Gr\\u00fcne ( \\u0111\\u1ea3ng Xanh ) hay \\u0111\\u1ea3ng Die_Linke ( C\\u00e1nh t\\u1ea3 ) \\u0111\\u1ec1u \\u0111ang nu\\u00f4i hy_v\\u1ecdng c\\u00f3 gh\\u1ebf t\\u1ea1i qu\\u1ed1c_h\\u1ed9i li\\u00ean_bang sau cu\\u1ed9c b\\u1ea7u_c\\u1eed n\\u00e0y .\", \"VKS \\u0111\\u1ec1_ngh\\u1ecb mi\\u1ec5n h\\u00ecnh_ph\\u1ea1t v\\u1edbi c\\u00e1c b\\u1ecb_c\\u00e1o : <person> Nguy\\u1ec5n_Vi\\u1ec7t_H\\u00e0 </person> ( 40 tu\\u1ed5i , gi\\u00e1m_\\u0111\\u1ed1c <organization> Ph\\u00f2ng giao_d\\u1ecbch \\u0110\\u00e0o_Duy_Anh </organization> , H\\u00e0_N\\u1ed9i ) ; Nguy\\u1ec5n_Phan_Trung_Ki\\u00ean ( 35 tu\\u1ed5i , nguy\\u00ean gi\\u00e1m_\\u0111\\u1ed1c chi_nh\\u00e1nh Ph\\u00f2ng giao_d\\u1ecbch \\u0110\\u00f4ng_\\u0110\\u00f4 , H\\u00e0_N\\u1ed9i ) ; Nguy\\u1ec5n_Th\\u1ecb_Loan ( 38 tu\\u1ed5i , nguy\\u00ean gi\\u00e1m_\\u0111\\u1ed1c chi_nh\\u00e1nh Ph\\u00f2ng giao_d\\u1ecbch Trung_Y\\u00ean ( H\\u00e0_N\\u1ed9i ) , Tr\\u1ea7n_V\\u0103n_Thi\\u1ebft ( nguy\\u00ean gi\\u00e1m_\\u0111\\u1ed1c chi_nh\\u00e1nh H\\u00e0_N\\u1ed9i ) .\", \"N\\u1ea1n_nh\\u00e2n_D\\u01b0\\u01a1ng t\\u1eed_vong sau khi b\\u1ecb t\\u00e0u SE6 t\\u00f4ng Theo \\u0111\\u00f3 , v\\u00e0o_kho\\u1ea3ng 12 gi\\u1edd 50 ph\\u00fat ng\\u00e0y 22-9 , t\\u00e0u kh\\u00e1ch mang s\\u1ed1_hi\\u1ec7u SE6 ch\\u1ea1y theo h\\u01b0\\u1edbng Nam-B\\u1eafc , khi \\u0111i t\\u1edbi km 344+850 , tr\\u01b0\\u1edbc <location> ga \\u0110\\u1ee9c L\\u1ea1c </location> ( thu\\u1ed9c \\u0111\\u1ecba_ph\\u1eadn th\\u00f4n Y\\u00ean_Th\\u1eafng , x\\u00e3 \\u0110\\u1ee9c_L\\u1ea1c , <location> huy\\u1ec7n \\u0110\\u1ee9c_Th\\u1ecd </location> ) th\\u00ec kh\\u00f4ng may x\\u1ea3y ra va_ch\\u1ea1m v\\u1edbi \\u00f4ng L\\u00ea_Qu\\u00fd_D .\", \"Trong th\\u1eddi_k\\u1ef3 th\\u1ebf_gi\\u1edbi tr\\u1ea3i qua nhi\\u1ec1u b\\u1ea5t_\\u1ed5n v\\u1edbi vi\\u1ec7c n\\u01b0\\u1edbc M\\u1ef9 do \\u00f4ng Donald_Trump l\\u00e3nh_\\u0111\\u1ea1o , v\\u1ea5n_\\u0111\\u1ec1 Brexit <location> ( Anh </location> r\\u1eddi <organization> Li\\u00ean_minh ch\\u00e2u_\\u00c2u </organization> - EU ) , c\\u00f9ng v\\u1edbi c\\u00e1c cu\\u1ed9c kh\\u1ee7ng_ho\\u1ea3ng to\\u00e0n_c\\u1ea7u , n\\u1eef l\\u00e3nh_\\u0111\\u1ea1o 63 tu\\u1ed5i n\\u00e0y \\u0111\\u00e3 tr\\u1edf_th\\u00e0nh ch\\u1ed7 d\\u1ef1a v\\u1eefng_ch\\u1eafc cho m\\u1ed9t \\u0111\\u1ea5t_n\\u01b0\\u1edbc d\\u00e0nh nhi\\u1ec1u quan_t\\u00e2m v\\u1ec1 v\\u1ea5n_\\u0111\\u1ec1 duy_tr\\u00ec t\\u1ed1c_\\u0111\\u1ed9 t\\u0103ng_tr\\u01b0\\u1edfng v\\u00e0 kh\\u1ea3_n\\u0103ng t\\u1ea1o c\\u00f4ng_\\u0103n_vi\\u1ec7c_l\\u00e0m mong_mu\\u1ed1n cho ng\\u01b0\\u1eddi_d\\u00e2n .\", \"D\\u00e2n_tr\\u00ed \\u0111\\u01b0a tin , ng\\u00e0y 21/9 , th\\u00f4ng_tin t\\u1eeb VKSND huy\\u1ec7n \\u0110\\u1eafk_R \\u2019 L\\u1ea5p ( t\\u1ec9nh \\u0110\\u1eafk_N\\u00f4ng ) cho bi\\u1ebft , \\u0111\\u01a1n_v\\u1ecb n\\u00e0y \\u0111\\u00e3 ph\\u00ea_chu\\u1ea9n quy\\u1ebft_\\u0111\\u1ecbnh kh\\u1edfi_t\\u1ed1 v\\u1ee5 \\u00e1n , kh\\u1edfi_t\\u1ed1 b\\u1ecb_can \\u0111\\u1ed1i_v\\u1edbi Chu_V\\u0103n_\\u0110\\u1ea1t ( SN 2000 ) v\\u00e0 <person> Nguy\\u1ec5n_H\\u1eefu_D\\u0169ng </person> ( SN 2002 ) \\u0111\\u1ec1u tr\\u00fa t\\u1ea1i th\\u00f4n 7 , x\\u00e3 \\u0110\\u1eafk_Wer , <location> huy\\u1ec7n \\u0110\\u1eafk_R \\u2019 L\\u1ea5p </location> \\u0111\\u1ec3 \\u0111i\\u1ec1u_tra v\\u1ec1 h\\u00e0nh_vi Tr\\u1ed9m_c\\u1eafp t\\u00e0i_s\\u1ea3n .\", \"Th\\u1ee7_t\\u01b0\\u1edbng \\u0110\\u1ee9c Angela_Merkel c\\u00f9ng ch\\u1ed3ng Joachim_Sauer ( tr\\u00e1i ) v\\u00e0 c\\u00e1c v\\u1ecb kh\\u00e1ch kh\\u00e1c n\\u00e2ng ly sau khi b\\u00e0 nh\\u1eadn Gi\\u1ea3i Leo_Baeck t\\u1ea1i <location> Kh\\u00e1ch_s\\u1ea1n Adlon </location> \\u1edf <location> Berlin </location> , \\u0110\\u1ee9c , ng\\u00e0y 6/11/2007 .\", \"( Ngoisao.net ) Trong m\\u1ed9t s\\u1ef1_ki\\u1ec7n m\\u1edbi_\\u0111\\u00e2y t\\u1ea1i <location> H\\u00e0_N\\u1ed9i </location> , Hoa_h\\u1eadu <person> Jennifer_Ph\\u1ea1m </person> g\\u00e2y b\\u1ea5t_ng\\u1edd khi v\\u1eeba l\\u00e0m MC v\\u1eeba tr\\u1ed5_t\\u00e0i ca_h\\u00e1t , khoe v\\u0169_\\u0111\\u1ea1o b\\u1ed1c_l\\u1eeda tr\\u00ean s\\u00e2n_kh\\u1ea5u .\", \"Trung_t\\u00e1 Hu\\u1ef3nh_Trung_Phong \\u2013 Tr\\u01b0\\u1edfng <organization> Ph\\u00f2ng CSGT \\u0110\\u01b0\\u1eddng_b\\u1ed9 - \\u0110\\u01b0\\u1eddng_s\\u1eaft </organization> <organization> C\\u00f4ng_an TP. HCM </organization> \\u0111\\u00e3 c\\u00f3 nh\\u1eefng tr\\u1ea3_l\\u1eddi c\\u1ee5_th\\u1ec3 v\\u1ec1 v\\u1ee5_vi\\u1ec7c CSGT b\\u1ecb t\\u1ed1 nh\\u1eadn h\\u1ed1i_l\\u1ed9 \\u1edf khu_v\\u1ef1c s\\u00e2n_bay T\\u00e2n_S\\u01a1n_Nh\\u1ea5t th\\u1eddi_gian qua .\", \"Th\\u01b0\\u01a1ng_hi\\u1ec7u <organization> Hanwha_Techwin </organization> s\\u1ebd l\\u00e0 n\\u1ec1n_t\\u1ea3ng , t\\u1ea1o hi\\u1ec7u_\\u1ee9ng lan_to\\u1ea3 , gi\\u00fap Khu C\\u00f4ng_ngh\\u1ec7_cao Ho\\u00e0_L\\u1ea1c c\\u00f3_th\\u1ec3 thu_h\\u00fat c\\u00e1c DN c\\u00f4ng_ngh\\u1ec7_cao kh\\u00e1c c\\u1ee7a <location> H\\u00e0n_Qu\\u1ed1c </location> v\\u00e0 c\\u00e1c qu\\u1ed1c_gia kh\\u00e1c \\u0111\\u1ebfn \\u0111\\u1ea7u_t\\u01b0 v\\u00e0 kinh_doanh trong th\\u1eddi_gian t\\u1edbi , g\\u00f3p_ph\\u1ea7n n\\u00e2ng cao ti\\u1ec1m_l\\u1ef1c khoa_h\\u1ecdc v\\u00e0 c\\u00f4ng_ngh\\u1ec7 c\\u1ee7a khu_v\\u1ef1c mi\\u1ec1n B\\u1eafc n\\u00f3i_ri\\u00eang v\\u00e0 Vi\\u1ec7t_Nam n\\u00f3i_chung .\", \"\\u00d4ng \\u0110\\u01b0\\u1ee3c \\u0111\\u01b0a ra l\\u1eadp_lu\\u1eadn : \\u201c \\u00d4ng \\u0110\\u1ed7_Anh_Qu\\u00e2n \\u0111\\u00e3 tr\\u00ecnh_b\\u00e0y vi\\u1ec7c gi\\u00e1m_\\u0111\\u1ecbnh \\u0111\\u01b0\\u1ee3c th\\u1ef1c_hi\\u1ec7n t\\u1ea1i tr\\u1ee5_s\\u1edf c\\u01a1_quan CS\\u0110T , tr\\u1ee5_s\\u1edf <organization> B\\u1ed9 C\\u00f4ng_an </organization> tr\\u00ean <location> \\u0111\\u01b0\\u1eddng Ph\\u1ea1m_V\\u0103n_\\u0110\\u1ed3ng </location> ( qu\\u1eadn C\\u1ea7u_Gi\\u1ea5y , H\\u00e0_N\\u1ed9i ) , ch\\u1ee9 kh\\u00f4ng nh\\u01b0 trong v\\u0103n_b\\u1ea3n ghi l\\u00e0 t\\u1ea1i C\\u01a1_quan thanh_tra NHNN ( s\\u1ed1 45 L\\u00fd th\\u01b0\\u1eddng Ki\\u1ec7t , qu\\u1eadn Ho\\u00e0n_Ki\\u1ebfm , H\\u00e0_N\\u1ed9i ) .\", \"Th\\u1eddi c\\u00f2n tr\\u1ebb kho\\u1ebb , \\u00f4ng <person> Kim </person> t\\u1eebng m\\u1edf <organization> phim_tr\\u01b0\\u1eddng Si\\u00eau_Qu\\u1ea7n </organization> v\\u00e0 C\\u00f4ng_ty gi\\u1ea3i_tr\\u00ed Si\\u00eau_Qu\\u1ea7n \\u0111\\u1ec3 l\\u00e0m c\\u00e1c b\\u1ed9 phim v\\u1ec1 Bao_C\\u00f4ng .\", \"B\\u00ean c\\u1ea1nh \\u0111\\u00f3 , ch\\u1ecb <person> Ho\\u00e0ng_H\\u00e0 </person> <location> ( H\\u00e0_N\\u1ed9i </location> ) \\u2013 \\u0111\\u00e3 k\\u1ebft_h\\u00f4n chia_s\\u1ebb th\\u00eam v\\u1ec1 c\\u00e2u_chuy\\u1ec7n l\\u01b0\\u01a1ng c\\u1ee7a ch\\u1ed3ng : \\u201c C\\u00e1ch \\u0111\\u00e2y 2 n\\u0103m , t\\u00f4i k\\u1ebft_h\\u00f4n , khi \\u1ea5y ch\\u1ed3ng t\\u00f4i l\\u01b0\\u01a1ng m\\u1edbi \\u0111\\u01b0\\u1ee3c 6 tri\\u1ec7u \\u0111\\u1ed3ng / th\\u00e1ng .\", \"L\\u00fac \\u0111\\u00f3 , t\\u00e0u c\\u00e1 QNg 90599 TS c\\u1ee7a \\u00f4ng Ao_Xu\\u00e2n_Ti\\u1ebfn ( SN 1971 ) \\u1edf <location> x\\u00e3 B\\u00ecnh_H\\u1ea3i </location> ( huy\\u1ec7n B\\u00ecnh_S\\u01a1n , <location> Qu\\u1ea3ng_Ng\\u00e3i </location> ) \\u0111ang di_chuy\\u1ec3n c\\u00e1ch \\u0111\\u00f3 kho\\u1ea3ng 11 h\\u1ea3i_l\\u00fd nh\\u1eadn \\u0111\\u01b0\\u1ee3c t\\u00edn_hi\\u1ec7u k\\u00eau c\\u1ee9u , l\\u1eadp_t\\u1ee9c \\u0111\\u1ebfn \\u1ee9ng_c\\u1ee9u .\", \"Ng\\u00e0y 22/9 , TAND huy\\u1ec7n C\\u01b0_M \\u2019 gar ( \\u0110\\u1eafk_L\\u1eafk ) \\u0111\\u00e3 m\\u1edf phi\\u00ean_to\\u00e0 x\\u00e9t_x\\u1eed s\\u01a1_th\\u1ea9m \\u0111\\u1ed1i_v\\u1edbi b\\u1ecb_c\\u00e1o Y \\u0102i_Adr\\u01a1ng ( SN 1985 , ng\\u1ee5 <location> bu\\u00f4n J\\u00f9 </location> , x\\u00e3 Ea_Tu , <location> TP Bu\\u00f4n_Ma_Thu\\u1ed9t </location> ) v\\u1ec1 t\\u1ed9i Hi\\u1ebfp_d\\u00e2m .\", \"Cu\\u1ed9c h\\u1ecdp c\\u00f3 s\\u1ef1 tham_d\\u1ef1 c\\u1ee7a Ngo\\u1ea1i_tr\\u01b0\\u1edfng <location> Hoa_K\\u1ef3 </location> <person> Rex_Tillerson </person> v\\u00e0 b\\u1ed9_tr\\u01b0\\u1edfng ngo\\u1ea1i_giao c\\u1ee7a nhi\\u1ec1u n\\u01b0\\u1edbc kh\\u00e1c .\", \"Tr\\u01b0\\u1edbc \\u0111\\u00f3 , c\\u1ef1u_binh G\\u1ea1c_Ma L\\u00ea_V\\u0103n Thoa \\u0111\\u00e3 c\\u00f3 \\u0111\\u01a1n g\\u1eedi\\u00a0S\\u1edf L\\u0110-TB-XH B\\u00ecnh_\\u0110\\u1ecbnh \\u0111\\u1ec1_ngh\\u1ecb gi\\u00e1m_\\u0111\\u1ecbnh l\\u1ea1i v\\u1ebft_th\\u01b0\\u01a1ng c\\u00f2n s\\u00f3t c\\u1ee7a \\u00f4ng trong tr\\u1eadn h\\u1ea3i_chi\\u1ebfn b\\u1ea3o_v\\u1ec7 <location> \\u0111\\u1ea3o G\\u1ea1c_Ma </location> ( thu\\u1ed9c Qu\\u1ea7n_\\u0111\\u1ea3o Tr\\u01b0\\u1eddng_Sa <location> - Vi\\u1ec7t_Nam </location> ) x\\u1ea3y ra v\\u00e0o th\\u00e1ng 3/1988.\", \"\\u0110\\u1ed3ng_b\\u00e0o R\\u1ee5c b\\u1ecb l\\u0169 v\\u00e2y 8 ng\\u00e0y sau b\\u00e3o Ng\\u00e0y 22-9 \\u00f4ng <person> Tr\\u1ea7n_Xu\\u00e2n_T\\u01b0 </person> , Tr\\u01b0\\u1edfng <location> b\\u1ea3n \\u00d3n </location> ( Th\\u01b0\\u1ee3ng_Ho\\u00e1 , Minh_Ho\\u00e1 , Qu\\u1ea3ng_B\\u00ecnh ) cho bi\\u1ebft \\u0111\\u1ebfn nay l\\u00e0 ng\\u00e0y th\\u1ee9 8 , 3 b\\u1ea3n \\u00d3n , Y\\u00ean_H\\u1ee3p , M\\u00f2_O_\\u1ed2_\\u1ed2 c\\u1ee7a \\u0111\\u1ed3ng_b\\u00e0o R\\u1ee5c v\\u1edbi h\\u01a1n 600 nh\\u00e2n_kh\\u1ea9u b\\u1ecb l\\u0169 v\\u00e2y .\", \"Theo Th\\u1ee9_tr\\u01b0\\u1edfng <organization> B\\u1ed9 N\\u00f4ng_nghi\\u1ec7p v\\u00e0 Ph\\u00e1t_tri\\u1ec3n n\\u00f4ng_th\\u00f4n </organization> <person> Tr\\u1ea7n_Thanh_Nam </person> , sau h\\u01a1n 3 n\\u0103m th\\u1ef1c_hi\\u1ec7n Quy\\u1ebft_\\u0111\\u1ecbnh s\\u1ed1 62/2013 \\u0111\\u00e3 b\\u1ed9c_l\\u1ed9 m\\u1ed9t_s\\u1ed1 b\\u1ea5t_c\\u1eadp , vi\\u1ec7c li\\u00ean_k\\u1ebft s\\u1ea3n_xu\\u1ea5t c\\u0169ng ch\\u1ec9 m\\u1edbi d\\u1eebng l\\u1ea1i \\u1edf \\u0111\\u1ed1i_t\\u01b0\\u1ee3ng c\\u00e2y l\\u00faa .\", \"Gia_\\u0111\\u00ecnh \\u0110\\u1ea1i_t\\u01b0\\u1edbng V\\u00f5_Nguy\\u00ean_Gi\\u00e1p t\\u1eb7ng qu\\u00e0 c\\u00e1c tr\\u01b0\\u1eddng_h\\u1ecdc b\\u1ecb \\u1ea3nh_h\\u01b0\\u1edfng b\\u1edfi b\\u00e3o s\\u1ed1 10 Ng\\u00e0y 22-9 , \\u0111\\u1ea1i_di\\u1ec7n gia_\\u0111\\u00ecnh \\u0110\\u1ea1i_t\\u01b0\\u1edbng V\\u00f5_Nguy\\u00ean_Gi\\u00e1p ph\\u1ed1i_h\\u1ee3p v\\u1edbi <organization> \\u0110\\u1ed3n BP Ro\\u00f2n </organization> , <organization> B\\u0110BP Qu\\u1ea3ng_B\\u00ecnh </organization> t\\u1ed5_ch\\u1ee9c trao_t\\u1eb7ng qu\\u00e0 cho c\\u00e1c tr\\u01b0\\u1eddng_h\\u1ecdc b\\u1ecb thi\\u1ec7t_h\\u1ea1i n\\u1eb7ng b\\u1edfi b\\u00e3o s\\u1ed1 10 tr\\u00ean \\u0111\\u1ecba_b\\u00e0n huy\\u1ec7n Qu\\u1ea3ng_Tr\\u1ea1ch , t\\u1ec9nh Qu\\u1ea3ng_B\\u00ecnh .\", \"Trong su\\u1ed1t 12 n\\u0103m qua , ch\\u00ednh_ph\\u1ee7 do <organization> CDU / CSU </organization> d\\u1eabn_d\\u1eaft , d\\u00f9 li\\u00ean_minh v\\u1edbi \\u0111\\u1ea3ng n\\u00e0o th\\u00ec c\\u0169ng \\u0111\\u1ec1u \\u0111\\u00e3 ho\\u00e0n_th\\u00e0nh t\\u1ed1t nhi\\u1ec7m_v\\u1ee5 \\u0111\\u01b0a n\\u01b0\\u1edbc <location> \\u0110\\u1ee9c </location> v\\u01b0\\u1ee3t qua h\\u1ebft kh\\u00f3_kh\\u0103n n\\u00e0y \\u0111\\u1ebfn kh\\u00f3_kh\\u0103n kh\\u00e1c , t\\u1eeb cu\\u1ed9c kh\\u1ee7ng_ho\\u1ea3ng_kinh_t\\u1ebf to\\u00e0n_c\\u1ea7u n\\u0103m 2008 , cu\\u1ed9c kh\\u1ee7ng_ho\\u1ea3ng ng\\u01b0\\u1eddi di_c\\u01b0 n\\u0103m 2015 , c\\u0169ng nh\\u01b0 \\\" c\\u00fa s\\u1ed1c \\\" n\\u01b0\\u1edbc Anh r\\u1eddi kh\\u1ecfi EU n\\u0103m 2016 .\", \"Gi\\u1ed1ng nh\\u01b0 s\\u1ed1 \\u0111\\u00f4ng sinh_vi\\u00ean th\\u1eddi \\u0111\\u00f3 , b\\u00e0 tham_gia phong_tr\\u00e0o \\u0111o\\u00e0n thanh_ni\\u00ean x\\u00e3_h\\u1ed9i_ch\\u1ee7_ngh\\u0129a , nh\\u01b0ng t\\u1eeb_ch\\u1ed1i l\\u1eddi \\u0111\\u1ec1_ngh\\u1ecb l\\u00e0m \\\" tay_trong \\\" cho B\\u1ed9 An_ninh qu\\u1ed1c_gia ( th\\u01b0\\u1eddng \\u0111\\u01b0\\u1ee3c bi\\u1ebft \\u0111\\u1ebfn l\\u00e0 <organization> Stasi </organization> ) , c\\u01a1_quan t\\u00ecnh_b\\u00e1o n\\u1ed9i_v\\u1ee5 v\\u00e0 h\\u1ea3i_ngo\\u1ea1i c\\u1ee7a <organization> C\\u1ed9ng_ho\\u00e0 D\\u00e2n_ch\\u1ee7 \\u0110\\u1ee9c </organization> ( \\u0110\\u00f4ng_\\u0110\\u1ee9c ) l\\u00fac b\\u1ea5y_gi\\u1edd .\", \"Sau khi \\u0103n xong , <person> B\\u1ea3o </person> ch\\u1edf v\\u1ee3 v\\u00e0o m\\u1ed9t con h\\u1ebbm tr\\u00ean \\u0111\\u01b0\\u1eddng L\\u1ea1c_Long_Qu\\u00e2n ( ph\\u01b0\\u1eddng 10 , <location> qu\\u1eadn T\\u00e2n_B\\u00ecnh </location> ) th\\u00ec ph\\u00e1t_hi\\u1ec7n m\\u1ed9t c\\u0103n nh\\u00e0 v\\u1eafng ch\\u1ee7 .\", \"Con c\\u00e1_\\u0111u\\u1ed1i n\\u00e0y n\\u1eb7ng 216kg , c\\u00f3 chi\\u1ec1u d\\u00e0i 3,2 m v\\u00e0 chi\\u1ec1u r\\u1ed9ng l\\u00e0 1,9 m .\", \"M\\u1ed9t s\\u0129_quan c\\u1ea3nh_s\\u00e1t Ho\\u00e0ng_gia Th\\u00e1i_Lan \\u0111\\u00e3 khai v\\u1edbi c\\u00e1c nh\\u00e0 \\u0111i\\u1ec1u_tra r\\u1eb1ng \\u00f4ng \\u0111\\u00e3 \\u201c \\u0111\\u01b0\\u1ee3c l\\u1ec7nh \\u201d gi\\u00fap c\\u1ef1u Th\\u1ee7_t\\u01b0\\u1edbng <person> Yingluck_Shinawatra </person> tr\\u1ed1n tho\\u00e1t kh\\u1ecfi <location> Th\\u00e1i_Lan </location> , theo Ph\\u00f3 Th\\u1ee7_t\\u01b0\\u1edbng Th\\u00e1i_Lan Prawit_Wongsuwon .\", \"Theo Ph\\u00f3 Ch\\u1ee7_t\\u1ecbch cao_c\\u1ea5p c\\u1ee7a <organization> Boeing </organization> t\\u1ea1i th\\u1ecb_tr\\u01b0\\u1eddng ch\\u00e2u_\\u00c1 - Th\\u00e1i_B\\u00ecnh_D\\u01b0\\u01a1ng v\\u00e0 <location> \\u1ea4n_\\u0110\\u1ed9 </location> , Dinesh_Keskar , t\\u0103ng_tr\\u01b0\\u1edfng s\\u1ed1_l\\u01b0\\u1ee3ng kh\\u00e1ch du_l\\u1ecbch \\u1edf khu_v\\u1ef1c n\\u00e0y s\\u1ebd \\u1edf m\\u1ee9c hai con_s\\u1ed1 , \\u0111\\u1eb7c_bi\\u1ec7t l\\u00e0 c\\u00e1c n\\u01b0\\u1edbc Malaysia , Vi\\u1ec7t_Nam , Th\\u00e1i_Lan v\\u00e0 Indonesia .\", \"\\u0110\\u00f3 l\\u00e0 t\\u00e2m_s\\u1ef1 c\\u1ee7a b\\u00e0 L\\u00e2m_Th\\u1ecb_T\\u1ea3n , 60 tu\\u1ed5i s\\u1ed1ng t\\u1ea1i <location> \\u0111\\u1ed9i 6 </location> <organization> h\\u1ee3p_t\\u00e1c_x\\u00e3 Ph\\u00fa_Th\\u1ecd </organization> , x\\u00e3 Ngh\\u0129a H\\u1ea3i , huy\\u1ec7n Ngh\\u0129a_H\\u01b0ng , t\\u1ec9nh Nam_\\u0110\\u1ecbnh v\\u1ec1 nh\\u1eefng n\\u0103m_th\\u00e1ng b\\u00e0 b\\u1ecb c\\u0103n_b\\u1ec7nh vi\\u00eam kh\\u1edbp h\\u00e0nh_h\\u1ea1 .\", \"\\u0110\\u1ed3ng_b\\u00e0o R\\u1ee5c b\\u1ecb l\\u0169 v\\u00e2y 8 ng\\u00e0y sau b\\u00e3o Ng\\u00e0y 22-9 \\u00f4ng Tr\\u1ea7n_Xu\\u00e2n_T\\u01b0 , Tr\\u01b0\\u1edfng <location> b\\u1ea3n \\u00d3n </location> <location> ( Th\\u01b0\\u1ee3ng_Ho\\u00e1 </location> , Minh_Ho\\u00e1 , Qu\\u1ea3ng_B\\u00ecnh ) cho bi\\u1ebft \\u0111\\u1ebfn nay l\\u00e0 ng\\u00e0y th\\u1ee9 8 , 3 b\\u1ea3n \\u00d3n , Y\\u00ean_H\\u1ee3p , M\\u00f2_O_\\u1ed2_\\u1ed2 c\\u1ee7a \\u0111\\u1ed3ng_b\\u00e0o R\\u1ee5c v\\u1edbi h\\u01a1n 600 nh\\u00e2n_kh\\u1ea9u b\\u1ecb l\\u0169 v\\u00e2y .\", \"Trong ph\\u1ea7n b\\u00e0o_ch\\u1eefa cho b\\u1ecb_c\\u00e1o Nguy\\u1ec5n_Xu\\u00e2n_S\\u01a1n , lu\\u1eadt_s\\u01b0 Nguy\\u1ec5n_Th\\u1ecb_Minh_Ph\\u01b0\\u01a1ng cho bi\\u1ebft , m\\u1ed9t_s\\u1ed1 t\\u00ecnh_ti\\u1ebft m\\u1edbi li\\u00ean_quan \\u0111\\u1ebfn h\\u00e0nh_vi c\\u1ee7a Nguy\\u1ec5n_Xu\\u00e2n_S\\u01a1n v\\u1eeba x\\u1ea3y ra l\\u00e0 vi\\u1ec7c kh\\u1edfi_t\\u1ed1 v\\u1ee5 \\u00e1n l\\u1ea1m_d\\u1ee5ng ch\\u1ee9c_v\\u1ee5 quy\\u1ec1n_h\\u1ea1n chi\\u1ebfm_\\u0111o\\u1ea1t t\\u00e0i_s\\u1ea3n x\\u1ea3y ra t\\u1ea1i Nga Vietsovpetro ( VSP ) , C\\u00f4ng_ty TNHH MTV L\\u1ecdc ho\\u00e1_d\\u1ea7u B\\u00ecnh_S\\u01a1n ( BSR ) , <organization> T\\u1ed5ng_c\\u00f4ng_ty Th\\u0103m_d\\u00f2 khai_th\\u00e1c D\\u1ea7u_kh\\u00ed </organization> <organization> ( PVEP </organization> ) .\", \"VKS c\\u0169ng \\u0111\\u1ec1 ngh\\u1ecb\\u00a0tuy\\u00ean m\\u1ed9t_s\\u1ed1 t\\u00ecnh_ti\\u1ebft gi\\u1ea3m nh\\u1eb9 cho <person> Ho\\u00e0ng_Th\\u1ecb_H\\u1ed3ng_T\\u1ee9 </person> ( c\\u1ef1u Ch\\u1ee7_t\\u1ecbch H\\u0110QT <organization> C\\u00f4ng_ty C\\u1ed5_ph\\u1ea7n BSC </organization> ) , x\\u1eed 30-36 th\\u00e1ng t\\u00f9 nh\\u01b0ng cho h\\u01b0\\u1edfng \\u00e1n_treo , th\\u1eddi_gian th\\u1eed_th\\u00e1ch 5 n\\u0103m .\", \"M\\u1edbi_\\u0111\\u00e2y , HLV tr\\u01b0\\u1edfng c\\u1ee7a <organization> MU </organization> - \\u00f4ng <person> Mourinho </person> \\u0111\\u00e3 quy\\u1ebft_\\u0111\\u1ecbnh ra_m\\u1eb7t nh\\u1eb1m n\\u00edu ch\\u00e2n ti\\u1ec1n_v\\u1ec7 ng\\u01b0\\u1eddi B\\u1ec9 - Marouane_Fellaini ti\\u1ebfp_t\\u1ee5c g\\u1eafn_b\\u00f3 v\\u1edbi MU .\", \"\\u0110\\u1eb7c_nhi\\u1ec7m Nga ra_tay di\\u1ec7t s\\u1ea1ch nh\\u00f3m phi\\u1ebfn_qu\\u00e2n \\u1edf Hama th\\u1ebf_n\\u00e0o ( video ) - \\u00a0H\\u00e3ng truy\\u1ec1n_th\\u00f4ng <organization> B\\u1ed9 Qu\\u1ed1c_ph\\u00f2ng Nga </organization> , <organization> TV Zvezda </organization> , c\\u00f4ng b\\u1ed1video ghi l\\u1ea1i cu\\u1ed9c t\\u1ea5n_c\\u00f4ng c\\u1ee7a l\\u1ef1c_l\\u01b0\\u1ee3ng \\u0111\\u1eb7c_nhi\\u1ec7m B\\u1ed9 qu\\u1ed1c ph\\u00f2ngNga , \\u0111\\u00e1nh_ti\\u00eau_di\\u1ec7t nh\\u00f3m chi\\u1ebfn_binh Hayat_Tahrir al-Sham ( HTS - nguy\\u00eanJabhat al-Nusra , chi_nh\\u00e1nh al-Qaeda Syria ) trong cu\\u1ed9c t\\u1ea5n_c\\u00f4ng gi\\u1ea3i v\\u00e2ytrung \\u0111\\u1ed9i qu\\u00e2n_c\\u1ea3nh Nga \\u1edf t\\u1ec9nh Idlib .\", \"V\\u1edbi tinh_th\\u1ea7n kh\\u1ea9n_tr\\u01b0\\u01a1ng , kh\\u00f4ng qu\\u1ea3n ng\\u00e0y_\\u0111\\u00eam , b\\u1eb1ng c\\u00e1c bi\\u1ec7n_ph\\u00e1p nghi\\u1ec7p_v\\u1ee5 , Ph\\u00f2ng PC45 \\u0111\\u00e3 nhanh_ch\\u00f3ng x\\u00e1c_\\u0111\\u1ecbnh \\u0111\\u1ed1i_t\\u01b0\\u1ee3ng <person> Nguy\\u1ec5n_Th\\u1ecb_Th\\u1ea3o </person> ( SN 1975 , tr\\u00fa t\\u1ea1i <location> x\\u00f3m \\u0110\\u00f4ng_B\\u00ecnh </location> , th\\u00f4n Du\\u1ec7_\\u0110\\u00f4ng , th\\u1ecb_tr\\u1ea5n Lim , huy\\u1ec7n Ti\\u00ean_Du , t\\u1ec9nh B\\u1eafc_Ninh ) c\\u00f3 nhi\\u1ec1u d\\u1ea5u_hi\\u1ec7u nghi_v\\u1ea5n li\\u00ean_quan tr\\u1ef1c_ti\\u1ebfp t\\u1edbi v\\u1ee5 \\u00e1n .\", \"Theo tin_t\\u1ee9c tr\\u00ean b\\u00e1o VnExpress , ng\\u00e0y 22/9 , <organization> C\\u00f4ng_an qu\\u1eadn 3 </organization> <location> ( TP HCM </location> ) cho bi\\u1ebft , \\u0111\\u01a1n_v\\u1ecb n\\u00e0y \\u0111ang t\\u1ea1m gi\\u1eef \\u0111\\u1ed1i_t\\u01b0\\u1ee3ng L\\u00ea_Kh\\u00f4i_Nguy\\u00ean ( 25 tu\\u1ed5i ) \\u0111\\u1ec3 \\u0111i\\u1ec1u_tra h\\u00e0nh_vi hu\\u1ef7_ho\\u1ea1i t\\u00e0i_s\\u1ea3n .\", \"Sinh n\\u0103m 1982 , hi\\u1ec7n l\\u00e0 chuy\\u00ean_gia t\\u00e2m_l\\u00fd tr\\u1ecb_li\\u1ec7u , t\\u00e1c_gi\\u1ea3 Kim_Th\\u00e0nh \\u0111\\u00e3 vi\\u1ebft cu\\u1ed1n \\\" D\\u1ea1y con t\\u1ef1 h\\u1ecdc \\\" \\u0111\\u1ec3_d\\u00e0nh t\\u1eb7ng tr\\u01b0\\u1edbc_ti\\u00ean cho c\\u00e1c con Quang_Kh\\u00e1nh , <person> Minh_\\u0110\\u1ee9c </person> , <person> Minh_L\\u00e2m </person> c\\u1ee7a m\\u00ecnh .\", \"Sau \\u0111\\u00f3 , <person> Th\\u1eafm </person> ch\\u1ec9_\\u0111\\u1ea1o <person> T\\u1ee9 </person> k\\u00fd nhi\\u1ec1u h\\u1ee3p_\\u0111\\u1ed3ng .\", \"( SN 1959 , Ch\\u1ee7_nhi\\u1ec7m h\\u1ee3p_t\\u00e1c_x\\u00e3 n\\u00f4ng_nghi\\u1ec7p th\\u00f4n Du\\u1ec7_\\u0110\\u00f4ng , thi tr\\u1ea5n Lim , huy\\u1ec7n Ti\\u00ean_Du , B\\u1eafc_Ninh ) c\\u00f3 d\\u1ea5u_hi\\u1ec7u b\\u1ecb s\\u00e1t_h\\u1ea1i d\\u00e3_man , c\\u01b0\\u1edbp t\\u00e0i_s\\u1ea3n , Ph\\u00f2ng C\\u1ea3nh_s\\u00e1t h\\u00ecnh_s\\u1ef1 <organization> ( PC 45 </organization> - <organization> C\\u00f4ng_an t\\u1ec9nh B\\u1eafc_Ninh </organization> ) \\u0111\\u00e3 th\\u00e0nh_l\\u1eadp H\\u1ed9i_\\u0111\\u1ed3ng kh\\u00e1m_nghi\\u1ec7m , ti\\u1ebfn_h\\u00e0nh kh\\u00e1m_nghi\\u1ec7m hi\\u1ec7n_tr\\u01b0\\u1eddng , kh\\u00e1m nghi\\u1ec7p t\\u1eed_thi , ph\\u1ed1i_h\\u1ee3p v\\u1edbi c\\u00e1c l\\u1ef1c_l\\u01b0\\u1ee3ng ch\\u1ee9c_n\\u0103ng t\\u1ed5_ch\\u1ee9c \\u0111i\\u1ec1u_tra l\\u00e0m r\\u00f5 v\\u1ee5_vi\\u1ec7c .\", \"\\u0110\\u1ec3 l\\u00e0m r\\u00f5 h\\u00e0nh_vi c\\u1ee7a \\u0111\\u1ed1i_t\\u01b0\\u1ee3ng Long , PV B\\u00e1o ANT\\u0110 \\u0111\\u00e3 c\\u00f3 bu\\u1ed5i trao_\\u0111\\u1ed5i c\\u1ee5_th\\u1ec3 v\\u1edbi Lu\\u1eadt_s\\u01b0 Tr\\u01b0\\u01a1ng_Anh_T\\u00fa \\u2013 <organization> V\\u0103n_ph\\u00f2ng Lu\\u1eadt_s\\u01b0 Tr\\u01b0\\u01a1ng_Anh_T\\u00fa </organization> , <organization> \\u0110o\\u00e0n Lu\\u1eadt_s\\u01b0 H\\u00e0_N\\u1ed9i </organization> .\", \"Th\\u1eddi_\\u0111i\\u1ec3m tr\\u00ean , t\\u1ed5 c\\u00f4ng_t\\u00e1c \\u0110\\u1ed9i CSGT s\\u1ed1 2 ( Ph\\u00f2ng CSGT Thanh_Ho\\u00e1 ) do \\u0110\\u1ea1i_u\\u00fd Ph\\u1ea1m_V\\u0103n_H\\u01b0ng l\\u00e0m t\\u1ed5_tr\\u01b0\\u1edfng \\u0111ang l\\u00e0m nhi\\u1ec7m_v\\u1ee5 t\\u1ea1i khu_v\\u1ef1c \\u0111\\u1ea7u c\\u1ea7u Lai_Th\\u00e0nh th\\u00ec ph\\u00e1t_hi\\u1ec7n xe m\\u00f4_t\\u00f4 mang BKS : 36 B 5-685.92 do <person> Ph\\u1ea1m_Th\\u1ecb_H\\u1eb1ng </person> ( SN 1994 , c\\u00f3 HKTT t\\u1ea1i ph\\u01b0\\u1eddng Ph\\u00fa_S\\u01a1n , <location> TP. </location>\", \"Theo <organization> C\\u1ee5c Vi\\u1ec5n_th\\u00f4ng </organization> <organization> ( B\\u1ed9 T T&T T </organization> ) , d\\u1ecbch_v\\u1ee5 chuy\\u1ec3n m\\u1ea1ng vi\\u1ec5n_th\\u00f4ng di_\\u0111\\u1ed9ng m\\u1eb7t_\\u0111\\u1ea5t gi\\u1eef nguy\\u00ean s\\u1ed1_l\\u00e0 d\\u1ecbch_v\\u1ee5 do doanh_nghi\\u1ec7p cung_c\\u1ea5p d\\u1ecbch_v\\u1ee5 th\\u00f4ng_tin di_\\u0111\\u1ed9ng m\\u1eb7t_\\u0111\\u1ea5t cung_c\\u1ea5p cho thu\\u00ea_bao di_\\u0111\\u1ed9ng c\\u1ee7a m\\u00ecnh , d\\u1ecbch_v\\u1ee5 n\\u00e0y cho_ph\\u00e9p thu\\u00ea_bao di_\\u0111\\u1ed9ng c\\u00f3_th\\u1ec3 chuy\\u1ec3n_\\u0111\\u1ed5i doanh_nghi\\u1ec7p cung_c\\u1ea5p d\\u1ecbch_v\\u1ee5 th\\u00f4ng_tin di_\\u0111\\u1ed9ng m\\u1eb7t_\\u0111\\u1ea5t m\\u00e0 v\\u1eabn gi\\u1eef nguy\\u00ean \\u0111\\u01b0\\u1ee3c s\\u1ed1 \\u0111i\\u1ec7n_tho\\u1ea1i c\\u1ee7a m\\u00ecnh bao_g\\u1ed3m c\\u1ea3 m\\u00e3 m\\u1ea1ng v\\u00e0 s\\u1ed1 thu\\u00ea_bao .\", \"Nh\\u00e0_l\\u00e3nh_\\u0111\\u1ea1o <location> Tri\\u1ec1u_Ti\\u00ean </location> <person> Kim Jong-un </person> .\", \"Ng\\u00e0y 22/9 , TAND t\\u1ec9nh C\\u00e0_Mau m\\u1edf phi\\u00ean_to\\u00e0 x\\u00e9t_x\\u1eed ph\\u00fac_th\\u1ea9m \\u0111\\u1ed1i_v\\u1edbi 8 b\\u1ecb_c\\u00e1o \\u201c tr\\u1ed9m s\\u00f2 \\u201d x\\u1ea3y ra t\\u1ea1i <location> B\\u00e3i_B\\u1ed3i </location> <location> ( huy\\u1ec7n N\\u0103m_C\\u0103n </location> , C\\u00e0_Mau ) .\", \"L\\u00fac \\u0111\\u00f3 , t\\u00e0u c\\u00e1 QNg 90599 TS c\\u1ee7a \\u00f4ng <person> Ao_Xu\\u00e2n_Ti\\u1ebfn </person> ( SN 1971 ) \\u1edf <location> x\\u00e3 B\\u00ecnh_H\\u1ea3i </location> ( huy\\u1ec7n B\\u00ecnh_S\\u01a1n , Qu\\u1ea3ng_Ng\\u00e3i ) \\u0111ang di_chuy\\u1ec3n c\\u00e1ch \\u0111\\u00f3 kho\\u1ea3ng 11 h\\u1ea3i_l\\u00fd nh\\u1eadn \\u0111\\u01b0\\u1ee3c t\\u00edn_hi\\u1ec7u k\\u00eau c\\u1ee9u , l\\u1eadp_t\\u1ee9c \\u0111\\u1ebfn \\u1ee9ng_c\\u1ee9u .\", \"L\\u00e2m_\\u0110\\u1ed3ng : C\\u1ea7m dao \\u0111\\u00e2m ch\\u1ebft ch\\u00fa do nghe ti\\u1ebfng c\\u00e3i nhau v\\u1edbi m\\u1eb9 M\\u1edbi_\\u0111\\u00e2y , TAND t\\u1ec9nh L\\u00e2m_\\u0110\\u1ed3ng \\u0111\\u00e3 tuy\\u00ean_ph\\u1ea1t b\\u1ecb_c\\u00e1o <person> \\u0110\\u00e0o_V\\u0129nh_An </person> ( 22 tu\\u1ed5i , tr\\u00fa t\\u1ea1i ph\\u01b0\\u1eddng 8 , <location> TP \\u0110\\u00e0_L\\u1ea1t </location> ) m\\u1ee9c \\u00e1n 5 n\\u0103m t\\u00f9 v\\u1ec1 t\\u1ed9i \\u201c Gi\\u1ebft ng\\u01b0\\u1eddi \\u201d .\", \"T\\u1ea1i th\\u1eddi_\\u0111i\\u1ec3m ki\\u1ec3m_tra t\\u00e0i_x\\u1ebf chi\\u1ebfc xe l\\u00e0 Nguy\\u1ec5n_M\\u1ea1nh_H\\u00e0 ( SN 1981 , tr\\u00fa t\\u1ea1i <location> huy\\u1ec7n Anh_S\\u01a1n </location> , <location> Ngh\\u1ec7_An </location> ) kh\\u00f4ng xu\\u1ea5t_tr\\u00ecnh \\u0111\\u01b0\\u1ee3c c\\u00e1c gi\\u1ea5y_t\\u1edd ch\\u1ee9ng_minh ngu\\u1ed3n_g\\u1ed1c c\\u1ee7a l\\u00f4 h\\u00e0ng tr\\u00ean .\", \"Xavi_Hernandez t\\u1eebng chia s\\u1ebd : \\u201c \\u0110\\u1ea1t \\u0111\\u1ebfn vinh_quang kh\\u00f4ng kh\\u00f3 , duy_tr\\u00ec \\u0111\\u01b0\\u1ee3c tr\\u00ean \\u0111\\u1ec9nh vinh_quang m\\u1edbi l\\u00e0 kh\\u00f3 . \\u201d Hay <person> Pep_Guardiola </person> sau khi \\u0111\\u01b0a <organization> Barcelona </organization> \\u0111o\\u1ea1t v\\u00f4_s\\u1ed1 danh_hi\\u1ec7u l\\u1edbn_nh\\u1ecf \\u0111\\u00e3 quy\\u1ebft_\\u0111\\u1ecbnh chia_tay \\u0111\\u1ed9i b\\u00f3ng x\\u1ee9 Catalunya .\", \"\\u1ea2nh : Vi_Thu_Tr\\u01b0\\u1edbc \\u0111\\u00f3 , v\\u00e0o l\\u00fac 23 gi\\u1edd 30 ph\\u00fat ng\\u00e0y 20-9 , t\\u1ea1i khu_v\\u1ef1c v\\u00f9ng_bi\\u1ec3n \\u0110\\u1ea7u T\\u00e1n , \\u0111\\u1ed9i tu\\u1ea7n_tra c\\u1ee7a H\\u1ea3i_\\u0111\\u1ed9i 2 , B\\u0110BP Qu\\u1ea3ng_Ninh ph\\u1ed1i_h\\u1ee3p v\\u1edbi \\u0110\\u1ed3n BP Tr\\u00e0 C\\u1ed5 ph\\u00e1t_hi\\u1ec7n v\\u00e0 b\\u1eaft_gi\\u1eef 1 b\\u00e8 g\\u1ed7 c\\u00f4ng_su\\u1ea5t 32CV tr\\u1ecdng_t\\u1ea3i 7 t\\u1ea5n , kh\\u00f4ng c\\u00f3 bi\\u1ec3n ki\\u1ec3m_so\\u00e1t do <person> \\u0110\\u1eb7ng_Tr\\u1ecdng_Tuy\\u00ean </person> , SN 1982 , tr\\u00fa t\\u1ea1i th\\u00f4n 3 , x\\u00e3 V\\u0129nh_Th\\u1ef1c , <location> TP M\\u00f3ng_C\\u00e1i </location> , t\\u1ec9nh Qu\\u1ea3ng_Ninh \\u0111i\\u1ec1u_khi\\u1ec3n \\u0111\\u1ed3ng_th\\u1eddi l\\u00e0 ch\\u1ee7 ph\\u01b0\\u01a1ng_ti\\u1ec7n .\", \"( phununews.vn ) Ch\\u1ec9 6 ng\\u00e0y sau khi c\\u00f4ng_b\\u1ed1 gh\\u00e9p th\\u1eadn th\\u00e0nh_c\\u00f4ng , <person> Selena_Gomez </person> \\u0111\\u00e3 xu\\u1ea5t_hi\\u1ec7n tr\\u00ean phim_tr\\u01b0\\u1eddng \\u0111\\u1ec3 ghi_h\\u00ecnh nh\\u1eefng c\\u1ea3nh quay l\\u00e3ng_m\\u1ea1n v\\u1edbi b\\u1ea1n di\\u1ec5n <person> Timothee_Chalamet </person> .\", \"VKS gi\\u1eef nguy\\u00ean quan_\\u0111i\\u1ec3m , b\\u1ecb_c\\u00e1o H\\u00e0_V\\u0103n_Th\\u1eafm ( c\\u1ef1u Ch\\u1ee7_t\\u1ecbch OceanBank ) l\\u00e0 \\u0111\\u1ed3ng_ph\\u1ea1m v\\u1edbi <person> Nguy\\u1ec5n_Xu\\u00e2n_S\\u01a1n </person> ( c\\u1ef1u TG\\u0110 <organization> OceanBank </organization> ) trong vi\\u1ec7c tham_\\u00f4 t\\u00e0i_s\\u1ea3n .\", \"V\\u00e0o ng\\u00e0y 8/3/1965 , b\\u00e3i bi\\u1ec3n <location> Xu\\u00e2n_Thi\\u1ec1u </location> \\u1edf gi\\u1eefa b\\u1edd <location> v\\u1ecbnh \\u0110\\u00e0_N\\u1eb5ng </location> l\\u00e0 n\\u01a1i l\\u00ednh \\u0111\\u00e1nh_b\\u1ed9 M\\u1ef9 l\\u1ea7n \\u0111\\u1ea7u_ti\\u00ean \\u0111\\u1ed5_b\\u1ed9 v\\u00e0o Vi\\u1ec7t_Nam , m\\u1ed9t d\\u1ea5u m\\u1ed1c kh\\u1edfi_\\u0111\\u1ea7u cho cu\\u1ed9c chi\\u1ebfn_tranh b\\u1ecb c\\u1ea3 th\\u1ebf_gi\\u1edbi l\\u00ean_\\u00e1n c\\u1ee7a ng\\u01b0\\u1eddi M\\u1ef9 .\", \"\\u0110ang tuy\\u1ec7t_v\\u1ecdng v\\u00ec ch\\u01b0a bi\\u1ebft ch\\u1eefa_tr\\u1ecb c\\u0103n_b\\u1ec7nh vi\\u00eam kh\\u1edbp nh\\u01b0_th\\u1ebf_n\\u00e0o th\\u00ec b\\u00e0 T\\u1ea3n \\u0111\\u1ecdc \\u0111\\u01b0\\u1ee3c tr\\u00ean Kho\\u1ebb 365 c\\u00f3 nhi\\u1ec1u b\\u1ec7nh_nh\\u00e2n b\\u1ecb vi\\u00eam \\u0111a kh\\u1edbp , c\\u00e1c b\\u1ec7nh v\\u1ec1 x\\u01b0\\u01a1ng kh\\u1edbp \\u0111\\u00e3 ch\\u1eefa kh\\u1ecfi b\\u1ec7nh nh\\u1edd b\\u00e0i_thu\\u1ed1c th\\u1ea3o_d\\u01b0\\u1ee3c c\\u1ee7a l\\u01b0\\u01a1ng_y Tri\\u1ec7u_Th\\u1ecb_B\\u00ecnh ( B\\u1ea3n Y\\u00ean_S\\u01a1n \\u2013 x\\u00e3 Ba_V\\u00ec \\u2013 <location> huy\\u1ec7n Ba_V\\u00ec </location> \\u2013 <location> H\\u00e0_N\\u1ed9i </location> ) .\", \"Tr\\u01b0\\u1edbc \\u0111\\u00f3 , v\\u00e0o 23h t\\u1ed1i 20/9 , do kh\\u00f4ng c\\u00f3 ti\\u1ec1n ti\\u00eau x\\u00e0i , <person> Quy\\u1ebft </person> n\\u1ea3y \\u00fd_\\u0111\\u1ecbnh \\u0111i t\\u1eeb <location> \\u0110\\u00e0_N\\u1eb5ng </location> ra \\u0111\\u1ecba_b\\u00e0n gi\\u00e1p_ranh l\\u00e0 huy\\u1ec7n Ph\\u00fa_L\\u1ed9c ( t\\u1ec9nh TT-Hu\\u1ebf ) ch\\u1edd gia_\\u0111\\u00ecnh n\\u00e0o c\\u00f3 s\\u01a1_h\\u1edf \\u0111\\u1ec3 ra_tay tr\\u1ed9m_c\\u1eafp t\\u00e0i_s\\u1ea3n .\", \"\\u1ea2nh : Vi_Thu_Tr\\u01b0\\u1edbc \\u0111\\u00f3 , v\\u00e0o l\\u00fac 23 gi\\u1edd 30 ph\\u00fat ng\\u00e0y 20-9 , t\\u1ea1i khu_v\\u1ef1c v\\u00f9ng_bi\\u1ec3n \\u0110\\u1ea7u T\\u00e1n , \\u0111\\u1ed9i tu\\u1ea7n_tra c\\u1ee7a H\\u1ea3i_\\u0111\\u1ed9i 2 , B\\u0110BP Qu\\u1ea3ng_Ninh ph\\u1ed1i_h\\u1ee3p v\\u1edbi \\u0110\\u1ed3n BP Tr\\u00e0 C\\u1ed5 ph\\u00e1t_hi\\u1ec7n v\\u00e0 b\\u1eaft_gi\\u1eef 1 b\\u00e8 g\\u1ed7 c\\u00f4ng_su\\u1ea5t 32CV tr\\u1ecdng_t\\u1ea3i 7 t\\u1ea5n , kh\\u00f4ng c\\u00f3 bi\\u1ec3n ki\\u1ec3m_so\\u00e1t do \\u0110\\u1eb7ng_Tr\\u1ecdng_Tuy\\u00ean , SN 1982 , tr\\u00fa t\\u1ea1i <location> th\\u00f4n 3 </location> , x\\u00e3 V\\u0129nh_Th\\u1ef1c , <location> TP M\\u00f3ng_C\\u00e1i </location> , t\\u1ec9nh Qu\\u1ea3ng_Ninh \\u0111i\\u1ec1u_khi\\u1ec3n \\u0111\\u1ed3ng_th\\u1eddi l\\u00e0 ch\\u1ee7 ph\\u01b0\\u01a1ng_ti\\u1ec7n .\", \"Tr\\u01b0\\u1edbc \\u0111\\u00f3 , anh <person> Ph\\u1ea1m_D\\u01b0_\\u0110\\u1ed3ng </person> ( SN 1988 , tr\\u00fa <location> x\\u00e3 S\\u01a1n_H\\u1ea3i </location> , huy\\u1ec7n B\\u1ea3o_Th\\u1eafng ) c\\u00f3 tr\\u00ecnh_b\\u00e1o v\\u00e0o_kho\\u1ea3ng 3h50 ng\\u00e0y 1/9 , anh \\u0111i\\u1ec1u_khi\\u1ec3n xe_m\\u00e1y \\u0111\\u1ebfn \\u0111\\u1ea7u c\\u1ea7u m\\u1edbi x\\u00e3 Gia_Ph\\u00fa , huy\\u1ec7n B\\u1ea3o_Th\\u1eafng \\u0111\\u1ec3 mua g\\u00e0 mang \\u0111i b\\u00e1n .\", \"Taekwondo Vi\\u1ec7t_Nam gi\\u00e0nh HCV \\u1edf AIMAG 2017 Chi\\u1ec1u nay , 3 v\\u1eadn_\\u0111\\u1ed9ng_vi\\u00ean n\\u1eef <location> Vi\\u1ec7t_Nam </location> l\\u00e0 Ch\\u00e2u_Tuy\\u1ebft_V\\u00e2n , Li\\u00ean_Th\\u1ecb_Tuy\\u1ebft_Mai v\\u00e0 <person> Nguy\\u1ec5n_Th\\u1ecb_L\\u1ec7_Kim </person> \\u0111\\u00e3 xu\\u1ea5t_s\\u1eafc gi\\u00e0nh huy_ch\\u01b0\\u01a1ng v\\u00e0ng quy\\u1ec1n \\u0111\\u1ed3ng_\\u0111\\u1ed9i n\\u1eef m\\u00f4n Taekwondo .\", \"\\u0110\\u00f3 l\\u00e0 t\\u00e2m_s\\u1ef1 c\\u1ee7a b\\u00e0 L\\u00e2m_Th\\u1ecb_T\\u1ea3n , 60 tu\\u1ed5i s\\u1ed1ng t\\u1ea1i \\u0111\\u1ed9i 6 <organization> h\\u1ee3p_t\\u00e1c_x\\u00e3 Ph\\u00fa_Th\\u1ecd </organization> , <location> x\\u00e3 Ngh\\u0129a H\\u1ea3i </location> , huy\\u1ec7n Ngh\\u0129a_H\\u01b0ng , t\\u1ec9nh Nam_\\u0110\\u1ecbnh v\\u1ec1 nh\\u1eefng n\\u0103m_th\\u00e1ng b\\u00e0 b\\u1ecb c\\u0103n_b\\u1ec7nh vi\\u00eam kh\\u1edbp h\\u00e0nh_h\\u1ea1 .\", \"Quan_h\\u1ec7 h\\u1ee3p_t\\u00e1c v\\u1edbi <location> t\\u1ec9nh V\\u00e2n_Nam </location> <location> ( Trung_Qu\\u1ed1c </location> ) v\\u00e0 c\\u00e1c t\\u1ec9nh B\\u1eafc_L\\u00e0o \\u0111\\u01b0\\u1ee3c quan_t\\u00e2m v\\u00e0 \\u0111\\u1ea1t nhi\\u1ec1u k\\u1ebft_qu\\u1ea3 thi\\u1ebft_th\\u1ef1c .\", \"Con_s\\u1ed1 tr\\u00ean th\\u1eadm_ch\\u00ed c\\u00f2n g\\u1ea7n g\\u1ea5p \\u0111\\u00f4i s\\u1ed1 ti\\u1ec1n Chelsea b\\u1ecf ra \\u0111\\u1ec3 mua <person> Costa </person> t\\u1eeb ch\\u00ednh <organization> Atletico </organization> h\\u1ed3i n\\u0103m 2014 .\", \"Khi xe \\u00f4_t\\u00f4 80 B-3487 v\\u1eeba d\\u1eebng t\\u1ea1i <location> ng\\u00f5 16 </location> , t\\u1ed5_d\\u00e2n_ph\\u1ed1 H\\u00e0_V\\u1ecb 2 , <location> ph\\u01b0\\u1eddng Th\\u1ecd_X\\u01b0\\u01a1ng </location> , Tp.\", \"Ng\\u00e0y 22/9 , TAND huy\\u1ec7n C\\u01b0_M \\u2019 gar ( \\u0110\\u1eafk_L\\u1eafk ) \\u0111\\u00e3 m\\u1edf phi\\u00ean_to\\u00e0 x\\u00e9t_x\\u1eed s\\u01a1_th\\u1ea9m \\u0111\\u1ed1i_v\\u1edbi b\\u1ecb_c\\u00e1o <person> Y \\u0102i_Adr\\u01a1ng </person> ( SN 1985 , ng\\u1ee5 <location> bu\\u00f4n J\\u00f9 </location> , x\\u00e3 Ea_Tu , TP Bu\\u00f4n_Ma_Thu\\u1ed9t ) v\\u1ec1 t\\u1ed9i Hi\\u1ebfp_d\\u00e2m .\", \"C\\u00f9ng ng\\u00e0y , Ph\\u00f3 Th\\u1ee7_t\\u01b0\\u1edbng , B\\u1ed9_tr\\u01b0\\u1edfng Ngo\\u1ea1i_giao Ph\\u1ea1m_B\\u00ecnh_Minh c\\u0169ng \\u0111\\u00e3 g\\u1eedi \\u0111i\\u1ec7n m\\u1eebng \\u0111\\u1ebfn B\\u1ed9_tr\\u01b0\\u1edfng <organization> B\\u1ed9 Ngo\\u1ea1i_giao Saudi_Arabia </organization> <person> Adel_Al_Jubeir </person> .\", \"<person> Suresh_Sivanandam </person> , chuy\\u00ean_vi\\u00ean t\\u01b0_v\\u1ea5n \\u1edf c\\u00f4ng_ty <organization> Wood_Mackenzie </organization> cho bi\\u1ebft \\u0111\\u1ee3t cung_c\\u1ea5p d\\u1ea7u_th\\u00f4 m\\u1edbi n\\u00e0y s\\u1ebd b\\u00f9 l\\u1ea1i ph\\u1ea7n ph\\u1ea7n ngu\\u1ed3n cung b\\u1ecb thi\\u1ebfu_h\\u1ee5t \\u1edf c\\u00e1c m\\u1ecf d\\u1ea7u l\\u00e2u n\\u0103m c\\u1ee7a Trung_Qu\\u1ed1c .\", \"* 15 h ng\\u00e0y 22/9 : U16 M\\u00f4ng_C\\u1ed5 \\u00a0 0-9 \\u00a0U 16 Vi\\u1ec7t_Nam ( s\\u00e2n\\u00a0MFF Football_Centre , \\u00a0Ulan Bator ) <organization> U16 Vi\\u1ec7t_Nam </organization> : <person> Nguy\\u00ean Ho\\u00e0ng </person> 13 ' , Thanh_Trung 24 ' ( pen ) , Ch\\u00ed_B\\u1ea3o 50 ' , 61 ' , 65 ' , 88 ' , Minh_\\u0110an 53 ' , Qu\\u1ed1c_Ho\\u00e0ng 57 ' , Thanh_H\\u1eadu 76 ' Tr\\u01b0\\u1edbc \\u0111\\u1ed9i ch\\u1ee7 nh\\u00e0 U16 M\\u00f4ng_C\\u1ed5 qu\\u00e1 y\\u1ebfu , U16 Vi\\u1ec7t_Nam kh\\u00f4ng g\\u1eb7p nhi\\u1ec1u kh\\u00f3_kh\\u0103n \\u0111\\u1ec3 gi\\u00e0nh chi\\u1ebfn_th\\u1eafng r\\u1ea5t \\u0111\\u1eadm .\", \"Theo \\u0111\\u1ea1i_s\\u1ee9 Nga , K\\u1ebf_ho\\u1ea1ch h\\u00e0nh_\\u0111\\u1ed9ng chung to\\u00e0n_di\\u1ec7n ( JCPOA ) - tho\\u1ea3_thu\\u1eadn l\\u1ecbch_s\\u1eed v\\u1ec1 h\\u1ea1t_nh\\u00e2n gi\\u1eefa Iran v\\u00e0 nh\\u00f3m P 5+1 c\\u1ee7a <organization> H\\u1ed9i_\\u0111\\u1ed3ng B\\u1ea3o_an Li\\u00ean_H\\u1ee3p_Qu\\u1ed1c </organization> g\\u1ed3m M\\u1ef9 , Nga , Trung_Qu\\u1ed1c , <location> Ph\\u00e1p </location> , Anh v\\u00e0 \\u0110\\u1ee9c \\u2013 l\\u00e0 bi\\u1ec3u_t\\u01b0\\u1ee3ng c\\u1ee7a chi\\u1ebfn_th\\u1eafng ngo\\u1ea1i_giao \\u0111a_ph\\u01b0\\u01a1ng , l\\u00e0 minh_ch\\u1ee9ng cho th\\u1ea5y ngay c\\u1ea3 nh\\u1eefng v\\u1ea5n_\\u0111\\u1ec1 qu\\u1ed1c_t\\u1ebf ph\\u1ee9c_t\\u1ea1p nh\\u1ea5t c\\u0169ng c\\u00f3_th\\u1ec3 \\u0111\\u01b0\\u1ee3c gi\\u1ea3i_quy\\u1ebft th\\u00f4ng_qua \\u0111\\u00e0m_ph\\u00e1n .\", \"N\\u1ebfu <person> Fellaini </person> kh\\u00f4ng s\\u1edbm \\u0111\\u1eb7t b\\u00fat k\\u00fd v\\u00e0o b\\u1ea3n h\\u1ee3p_\\u0111\\u1ed3ng m\\u1edbi th\\u00ec bu\\u1ed9c <organization> MU </organization> s\\u1ebd ph\\u1ea3i b\\u00e1n anh tr\\u01b0\\u1edbc khi c\\u1ea7u_th\\u1ee7 n\\u00e0y c\\u00f3_th\\u1ec3 ra \\u0111i theo d\\u1ea1ng t\\u1ef1_do .\", \"Angela_Merkel - V\\u1ecb ' Th\\u1ee7_t\\u01b0\\u1edbng b\\u1ea5t_di\\u1ec7t ' Th\\u1ee7_t\\u01b0\\u1edbng \\u0110\\u1ee9c <person> Angela_Merkel </person> \\u0111\\u01b0\\u1ee3c v\\u00ed_nh\\u01b0 \\\" n\\u1eef_ho\\u00e0ng kh\\u1eafc_kh\\u1ed5 \\\" c\\u1ee7a <location> ch\\u00e2u_\\u00c2u </location> , \\u0111\\u01b0\\u1ee3c nh\\u1eefng ng\\u01b0\\u1eddi t\\u1ecb_n\\u1ea1n ca_ng\\u1ee3i l\\u00e0 v\\u1ecb c\\u1ee9u_tinh v\\u00e0 \\u0111\\u01b0\\u1ee3c \\u0111\\u00e1nh_gi\\u00e1 l\\u00e0 \\\" nh\\u00e0_l\\u00e3nh_\\u0111\\u1ea1o m\\u1edbi c\\u1ee7a th\\u1ebf_gi\\u1edbi t\\u1ef1_do \\\" .\", \"Kh\\u1edfi_t\\u1ed1 v\\u1ee5 \\u00e1n ph\\u00e1 r\\u1eebng_ph\\u00f2ng_h\\u1ed9 \\u1edf <location> Qu\\u1ea3ng_Nam </location> <organization> C\\u00f4ng_an t\\u1ec9nh Qu\\u1ea3ng_Nam </organization> \\u0111\\u00e3 ch\\u00ednh_th\\u1ee9c kh\\u1edfi_t\\u1ed1 v\\u1ee5 \\u00e1n ph\\u00e1 r\\u1eebng_ph\\u00f2ng_h\\u1ed9 \\u1edf Ti\\u00ean_L\\u00e3nh ( Qu\\u1ea3ng_Nam ) v\\u00e0 \\u0111ang ho\\u00e0n_thi\\u1ec7n h\\u1ed3_s\\u01a1 \\u0111\\u1ec3 kh\\u1edfi_t\\u1ed1 b\\u1ecb_can .\", \"( SN 1959 , Ch\\u1ee7_nhi\\u1ec7m h\\u1ee3p_t\\u00e1c_x\\u00e3 n\\u00f4ng_nghi\\u1ec7p th\\u00f4n Du\\u1ec7_\\u0110\\u00f4ng , <location> thi tr\\u1ea5n Lim </location> , huy\\u1ec7n Ti\\u00ean_Du , <location> B\\u1eafc_Ninh </location> ) c\\u00f3 d\\u1ea5u_hi\\u1ec7u b\\u1ecb s\\u00e1t_h\\u1ea1i d\\u00e3_man , c\\u01b0\\u1edbp t\\u00e0i_s\\u1ea3n , Ph\\u00f2ng C\\u1ea3nh_s\\u00e1t h\\u00ecnh_s\\u1ef1 ( PC 45 - C\\u00f4ng_an t\\u1ec9nh B\\u1eafc_Ninh ) \\u0111\\u00e3 th\\u00e0nh_l\\u1eadp H\\u1ed9i_\\u0111\\u1ed3ng kh\\u00e1m_nghi\\u1ec7m , ti\\u1ebfn_h\\u00e0nh kh\\u00e1m_nghi\\u1ec7m hi\\u1ec7n_tr\\u01b0\\u1eddng , kh\\u00e1m nghi\\u1ec7p t\\u1eed_thi , ph\\u1ed1i_h\\u1ee3p v\\u1edbi c\\u00e1c l\\u1ef1c_l\\u01b0\\u1ee3ng ch\\u1ee9c_n\\u0103ng t\\u1ed5_ch\\u1ee9c \\u0111i\\u1ec1u_tra l\\u00e0m r\\u00f5 v\\u1ee5_vi\\u1ec7c .\", \"Khu_v\\u1ef1c <location> mi\\u1ec1n Nam </location> <location> Th\\u00e1i_Lan </location> th\\u01b0\\u1eddng x\\u1ea3y ra t\\u00ecnh_tr\\u1ea1ng xung_\\u0111\\u1ed9t do m\\u1ed9t_s\\u1ed1 nh\\u00f3m ly_khai ho\\u1ea1t_\\u0111\\u1ed9ng nh\\u1eb1m \\u0111\\u00f2i thi\\u1ebft_l\\u1eadp m\\u1ed9t nh\\u00e0_n\\u01b0\\u1edbc H\\u1ed3i_gi\\u00e1o ri\\u00eang .\", \"B\\u1ed9_tr\\u01b0\\u1edfng <person> T\\u00f4_L\\u00e2m </person> nh\\u1ea5n_m\\u1ea1nh , <organization> B\\u1ed9 C\\u00f4ng_an Vi\\u1ec7t_Nam </organization> \\u0111\\u00e3 thi\\u1ebft_l\\u1eadp quan_h\\u1ec7 h\\u1ee3p_t\\u00e1c , hi\\u1ec7u_qu\\u1ea3 v\\u1edbi c\\u00e1c c\\u01a1_quan th\\u1ef1c_thi ph\\u00e1p_lu\\u1eadt c\\u1ee7a Th\\u1ed5_Nh\\u0129_K\\u1ef3 .\", \"( SN 1977 , qu\\u00ea t\\u1ea1i th\\u00f4n \\u0110anh , x\\u00e3 B\\u1eb1ng_An , <location> huy\\u1ec7n Qu\\u1ebf_V\\u00f5 </location> , <location> t\\u1ec9nh B\\u1eafc_Ninh </location> ; tr\\u00fa t\\u1ea1i Ng\\u00f5 16 , t\\u1ed5_d\\u00e2n_ph\\u1ed1 H\\u00e0_V\\u1ecb 2 , ph\\u01b0\\u1eddng Th\\u1ecd_X\\u01b0\\u01a1ng , Tp.\", \"N\\u1eef hung_th\\u1ee7 th\\u1eeba_nh\\u1eadn \\u0111\\u00e3 d\\u00f9ng ch\\u00e0y nh\\u00f4m s\\u00e1t_h\\u1ea1i Ch\\u1ee7_nhi\\u1ec7m H\\u1ee3p_t\\u00e1c_x\\u00e3 Sau nhi\\u1ec1u ng\\u00e0y \\u0111i\\u1ec1u_tra , C\\u00f4ng_an t\\u1ec9nh B\\u1eafc_Ninh \\u0111\\u00e3 c\\u00f3 \\u0111\\u1ee7 ch\\u1ee9ng_c\\u1ee9 kh\\u1eb3ng_\\u0111\\u1ecbnh <person> Nguy\\u1ec5n_Th\\u1ecb_Th\\u1ea3o </person> ( SN 1975 ) , tr\\u00fa t\\u1ea1i th\\u00f4n Du\\u1ec7_\\u0110\\u00f4ng , th\\u1ecb_tr\\u1ea5n Lim , <location> huy\\u1ec7n Ti\\u00ean_Du </location> , B\\u1eafc_Ninh l\\u00e0 k\\u1ebb ra_tay gi\\u1ebft ch\\u1ebft b\\u00e0 Hu\\u1ea5n .\", \"V\\u1ec1 h\\u00e0nh_vi tham_\\u00f4 s\\u1ed1 ti\\u1ec1n 49 t\\u1ef7 \\u0111\\u1ed3ng c\\u1ee7a Nguy\\u1ec5n_Xu\\u00e2n_S\\u01a1n , VKS c\\u0103n_c\\u1ee9 v\\u00e0o quy_\\u0111\\u1ecbnh c\\u1ee7a lu\\u1eadt Doanh_nghi\\u1ec7p , x\\u00e1c_\\u0111\\u1ecbnh <person> S\\u01a1n </person> l\\u00e0 \\u0111\\u1ea1i_di\\u1ec7n v\\u1ed1n g\\u00f3p c\\u1ee7a PVN t\\u1ea1i <organization> OceanBank </organization> .\", \"L\\u00fac n\\u00e0y <person> Nguy\\u1ec5n_Nh\\u1eadt_Anh </person> ( SN 2002 , ng\\u1ee5 <location> \\u1ea5p Ph\\u01b0\\u1edbc_Th\\u1ea1nh </location> , x\\u00e3 L\\u1ed9c_Ho\\u00e0 , huy\\u1ec7n Long_H\\u1ed3 ) \\u0111i t\\u1eeb trong ti\\u1ec7m internet ra th\\u1ea5y Tr\\u01b0\\u1eddng l\\u00e0 \\u201c t\\u00ecnh_\\u0111\\u1ecbch \\u201d c\\u1ee7a m\\u00ecnh n\\u00ean Nh\\u1eadt_Anh k\\u00eau Tr\\u01b0\\u1eddng ra ngo\\u00e0i \\u201c n\\u00f3i_chuy\\u1ec7n \\u201d .\", \"Vi\\u1ec7c b\\u00e0 <person> Merkel </person> , m\\u1ed9t ng\\u01b0\\u1eddi ph\\u1ee5_n\\u1eef t\\u1eeb <location> \\u0110\\u00f4ng_\\u0110\\u1ee9c </location> , tr\\u1edf_th\\u00e0nh th\\u00e0nh_vi\\u00ean c\\u1ee7a CDU v\\u00e0 v\\u01b0\\u01a1n l\\u00ean v\\u1ecb_tr\\u00ed \\u0111\\u1ee9ng \\u0111\\u1ea7u khi\\u1ebfn nhi\\u1ec1u ng\\u01b0\\u1eddi b\\u1ea5t_ng\\u1edd .\", \"C\\u0169ng li\\u00ean_quan \\u0111\\u1ebfn h\\u00e0nh_vi v\\u1eadn_chuy\\u1ec3n ma_tu\\u00fd , b\\u00e1o C\\u00f4ng_l\\u00fd th\\u00f4ng_tin th\\u00eam , kho\\u1ea3ng 14h45 ng\\u00e0y 21/9 , t\\u1ed5 c\\u00f4ng_t\\u00e1c c\\u0169ng ph\\u00e1t_hi\\u1ec7n Ph\\u1ea1m_V\\u0103n_Kh\\u1ea3i ( SN 1993 , \\u1edf x\\u00e3 H\\u01b0ng_L\\u1ed9c , <location> huy\\u1ec7n H\\u1eadu_L\\u1ed9c </location> , <location> Thanh_Ho\\u00e1 </location> ) gi\\u1ea5u 3,27 g heroin v\\u00e0 4,32 g ma_tu\\u00fd d\\u1ea1ng \\u0111\\u00e1 trong bao thu\\u1ed1c_l\\u00e1 Th\\u0103ng_Long khi \\u0111ang di_chuy\\u1ec3n tr\\u00ean xe taxi mang BKS 36 A-166 . 87 t\\u1ea1i ph\\u01b0\\u1eddng Qu\\u1ea3ng_Th\\u00e0nh , TP.\", \"\\u0110\\u00f3 l\\u00e0 t\\u00e2m_s\\u1ef1 c\\u1ee7a b\\u00e0 L\\u00e2m_Th\\u1ecb_T\\u1ea3n , 60 tu\\u1ed5i s\\u1ed1ng t\\u1ea1i <location> \\u0111\\u1ed9i 6 </location> h\\u1ee3p_t\\u00e1c_x\\u00e3 Ph\\u00fa_Th\\u1ecd , x\\u00e3 Ngh\\u0129a H\\u1ea3i , <location> huy\\u1ec7n Ngh\\u0129a_H\\u01b0ng </location> , t\\u1ec9nh Nam_\\u0110\\u1ecbnh v\\u1ec1 nh\\u1eefng n\\u0103m_th\\u00e1ng b\\u00e0 b\\u1ecb c\\u0103n_b\\u1ec7nh vi\\u00eam kh\\u1edbp h\\u00e0nh_h\\u1ea1 .\", \"Danh_t\\u00ednh nghi can g\\u00e2y ra v\\u1ee5 \\u00e1n c\\u01b0\\u1edbp c\\u1ee7a , s\\u00e1t_h\\u1ea1i b\\u00e0 Nguy\\u1ec5n_Th\\u1ecb_H . , Ph\\u00f3 ch\\u1ee7_nhi\\u1ec7m HTX th\\u1ecb_tr\\u1ea5n Lim , Ti\\u00ean_Du ( B\\u1eafc_Ninh ) l\\u00e0\\u00a0Nguy\\u1ec5n Th\\u1ecb_Th\\u1ea3o ( SN 1975 , tr\\u00fa t\\u1ea1i x\\u00f3m \\u0110\\u00f4ng_B\\u00ecnh , <location> th\\u00f4n Du\\u1ec7_\\u0110\\u00f4ng </location> , <location> th\\u1ecb_tr\\u1ea5n Lim </location> , huy\\u1ec7n Ti\\u00ean_Du , t\\u1ec9nh B\\u1eafc_Ninh ) .\", \"<person> Karl_Vick </person> , t\\u1ed5ng_bi\\u00ean_t\\u1eadp c\\u1ee7a <organization> Time </organization> , m\\u00f4_t\\u1ea3 b\\u00e0 l\\u00e0 \\\" l\\u00e3nh_\\u0111\\u1ea1o th\\u1ef1c_quy\\u1ec1n c\\u1ee7a Li\\u00ean_minh ch\\u00e2u_\\u00c2u \\\" khi l\\u00e0 ng\\u01b0\\u1eddi \\u0111\\u1ee9ng \\u0111\\u1ea7u qu\\u1ed1c_gia th\\u00e0nh_vi\\u00ean l\\u1edbn nh\\u1ea5t v\\u00e0 gi\\u00e0u_m\\u1ea1nh nh\\u1ea5t c\\u1ee7a EU .\", \"Ng\\u00e0y 22/9 , <organization> C\\u00f4ng_an huy\\u1ec7n Ph\\u00fa_B\\u00ecnh </organization> , <location> t\\u1ec9nh Th\\u00e1i_Nguy\\u00ean </location> cho bi\\u1ebft , tr\\u00ean \\u0111\\u1ecba_b\\u00e0n v\\u1eeba x\\u1ea3y ra v\\u1ee5_vi\\u1ec7c hai v\\u1ee3_ch\\u1ed3ng ch\\u1ebft b\\u1ea5t_th\\u01b0\\u1eddng t\\u1ea1i ph\\u00f2ng ng\\u1ee7 nh\\u00e0_ri\\u00eang , b\\u01b0\\u1edbc_\\u0111\\u1ea7u x\\u00e1c_\\u0111\\u1ecbnh ng\\u01b0\\u1eddi ch\\u1ed3ng \\u0111\\u00e3 gi\\u1ebft v\\u1ee3 sau \\u0111\\u00f3 u\\u1ed1ng thu\\u1ed1c di\\u1ec7t c\\u1ecf t\\u1ef1_t\\u1eed .\", \"\\u0110\\u00e2y l\\u00e0 s\\u1ef1_ki\\u1ec7n quan_tr\\u1ecdng \\u0111\\u1ed1i_v\\u1edbi ng\\u00e0nh ch\\u0103n_nu\\u00f4i g\\u00e0 v\\u00f9ng <location> \\u0110BSCL </location> n\\u00f3i_ri\\u00eang , <location> Vi\\u1ec7t_Nam </location> n\\u00f3i_chung .\", \"\\u0110\\u1ec3 b\\u00e0o_ch\\u1eefa cho 4 b\\u1ecb_c\\u00e1o l\\u00e0 c\\u00e1c Gi\\u00e1m_\\u0111\\u1ed1c chi_nh\\u00e1nh OceanBank , lu\\u1eadt_s\\u01b0 Ho\\u00e0ng_Huy_\\u0110\\u01b0\\u1ee3c \\u0111\\u00e3 vi\\u1ec7n_d\\u1eabn : \\\" Th\\u1ef1c_t\\u1ebf sau khi c\\u00f3 Th\\u00f4ng_t\\u01b0 02 v\\u00e0 Ch\\u1ec9_th\\u1ecb 02 , theo \\u00f4ng <person> Tr\\u1ea7n_Anh_H\\u00f9ng </person> , Ph\\u00f3 Gi\\u00e1m_\\u0111\\u1ed1c <organization> NHNN </organization> Chi_nh\\u00e1nh H\\u1ea3i_D\\u01b0\\u01a1ng , l\\u00e0 ng\\u01b0\\u1eddi \\u0111\\u1ea1i_di\\u1ec7n theo u\\u1ef7_quy\\u1ec1n c\\u1ee7a Th\\u1ed1ng_\\u0111\\u1ed1c \\u0111\\u01b0\\u01a1ng_nhi\\u1ec7m L\\u00ea_Minh_H\\u01b0ng t\\u1ea1i phi\\u00ean_to\\u00e0 x\\u00e9t_x\\u1eed \\u0111\\u1ea1i \\u00e1n OceanBank , NHNN \\u0111\\u00e3 x\\u1eed_l\\u00fd h\\u00e0nh_ch\\u00ednh \\u0111\\u1ed1i_v\\u1edbi 02 ng\\u00e2n_h\\u00e0ng l\\u00e0 HDBank v\\u00e0 MaritimeBank \\\" .\", \"Trong l\\u00fac tham_gia l\\u1ec5 quy\\u00ean_g\\u00f3p t\\u1eeb_thi\\u1ec7n cho qu\\u1ef9 nghi\\u00ean_c\\u1ee9u ph\\u00f2ng_ch\\u1ed1ng ung_th\\u01b0 v\\u00fa , <person> R\\u00f4 b\\u00e9o </person> \\u0111\\u00e3 b\\u00ecnh_lu\\u1eadn v\\u1ec1 kh\\u00f3_kh\\u0103n c\\u1ee7a \\u0111\\u1ed9i b\\u00f3ng c\\u0169 : <organization> \\\" Real </organization> c\\u00f3 r\\u1ea5t nhi\\u1ec1u c\\u1ea7u_th\\u1ee7 c\\u00f3_th\\u1ec3 ghi_b\\u00e0n v\\u00e0 t\\u00f4i tin h\\u1ecd kh\\u00f4ng thi\\u1ebfu b\\u1ea5t_c\\u1ee9 th\\u1ee9 g\\u00ec trong v\\u00e0i n\\u0103m qua .\", \"Premier_League \\u0111\\u00e3 chia_tay m\\u1ed9t trong nh\\u1eefng ti\\u1ec1n_\\u0111\\u1ea1o hay nh\\u1ea5t <person> Diego_Costa </person> cu\\u1ed1i_c\\u00f9ng \\u0111\\u00e3 tr\\u1edf_l\\u1ea1i <organization> Atletico_Madrid </organization> t\\u1eeb Chelsea v\\u1edbi ph\\u00ed chuy\\u1ec3n_nh\\u01b0\\u1ee3ng 55 tri\\u1ec7u euro .\", \"Gi\\u1ed1ng nh\\u01b0 s\\u1ed1 \\u0111\\u00f4ng sinh_vi\\u00ean th\\u1eddi \\u0111\\u00f3 , b\\u00e0 tham_gia phong_tr\\u00e0o \\u0111o\\u00e0n thanh_ni\\u00ean x\\u00e3_h\\u1ed9i_ch\\u1ee7_ngh\\u0129a , nh\\u01b0ng t\\u1eeb_ch\\u1ed1i l\\u1eddi \\u0111\\u1ec1_ngh\\u1ecb l\\u00e0m \\\" tay_trong \\\" cho B\\u1ed9 An_ninh qu\\u1ed1c_gia ( th\\u01b0\\u1eddng \\u0111\\u01b0\\u1ee3c bi\\u1ebft \\u0111\\u1ebfn l\\u00e0 <organization> Stasi </organization> ) , c\\u01a1_quan t\\u00ecnh_b\\u00e1o n\\u1ed9i_v\\u1ee5 v\\u00e0 h\\u1ea3i_ngo\\u1ea1i c\\u1ee7a C\\u1ed9ng_ho\\u00e0 D\\u00e2n_ch\\u1ee7 \\u0110\\u1ee9c <location> ( \\u0110\\u00f4ng_\\u0110\\u1ee9c </location> ) l\\u00fac b\\u1ea5y_gi\\u1edd .\", \"Ng\\u00e0y 22/9 , TAND huy\\u1ec7n C\\u01b0_M \\u2019 gar ( \\u0110\\u1eafk_L\\u1eafk ) \\u0111\\u00e3 m\\u1edf phi\\u00ean_to\\u00e0 x\\u00e9t_x\\u1eed s\\u01a1_th\\u1ea9m \\u0111\\u1ed1i_v\\u1edbi b\\u1ecb_c\\u00e1o <person> Y \\u0102i_Adr\\u01a1ng </person> ( SN 1985 , ng\\u1ee5 bu\\u00f4n J\\u00f9 , x\\u00e3 Ea_Tu , <location> TP Bu\\u00f4n_Ma_Thu\\u1ed9t </location> ) v\\u1ec1 t\\u1ed9i Hi\\u1ebfp_d\\u00e2m .\", \"\\u0110\\u1ed3ng_th\\u1eddi \\u0111\\u00e3 c\\u00f3 2 l\\u1ea7n t\\u1eeb v\\u1ecb_tr\\u00ed c\\u1ee7a SDF , ph\\u00e1o , c\\u1ed1i , t\\u00ean_l\\u1eeda nh\\u1eb1m v\\u00e0o SAA v\\u00e0 \\u0111\\u1eb7c_nhi\\u1ec7m Nga nh\\u1ea3 \\u0111\\u1ea1n \\u2026 3 , M\\u1ef9 s\\u1eed_d\\u1ee5ng l\\u1ef1c_l\\u01b0\\u1ee3ng Al_Qeada , HTS t\\u1ea1i Idlid m\\u1edf_m\\u1eb7t tr\\u1eadn B\\u1eafc_Hama ( trong \\u0111\\u00f3 c\\u00f3 m\\u1ed9t \\u0111\\u00f2n \\u0111\\u1ed9c : B\\u1eaft s\\u1ed1ng 29 c\\u1ea3nh_s\\u00e1t qu\\u00e2n_s\\u1ef1 Nga ) h\\u00f2ng k\\u00e9o gi\\u1ea3n l\\u1ef1c_l\\u01b0\\u1ee3ng <organization> SAA </organization> kh\\u1ecfi <location> Deir_Ezzor </location> \\u2026\", \"Nh\\u00e0_l\\u00e3nh_\\u0111\\u1ea1o <person> Tri\\u1ec1u_Ti\\u00ean </person> <person> Kim Jong-un </person> c\\u1ea7m m\\u1ed9t chi\\u1ebfc ba_l\\u00f4 h\\u1ecdc_sinh khi t\\u1edbi th\\u0103m nh\\u00e0_m\\u00e1y d\\u1ec7t Kim Jong-suk \\u1edf th\\u1ee7_\\u0111\\u00f4 B\\u00ecnh_Nh\\u01b0\\u1ee1ng .\", \"\\u0111\\u1ebfn <organization> B\\u1ec7nh_vi\\u1ec7n ( BV ) ph\\u1eabu_thu\\u1eadt th\\u1ea9m_m\\u1ef9 EMCSA </organization> ( s\\u1ed1 14/27 Ho\\u00e0ng_D\\u01b0_Kh\\u01b0\\u01a1ng , P. 12 , <location> Q. 10 </location> , TP. HCM ) m\\u1ed5 ch\\u1ec9nh x\\u01b0\\u01a1ng hai h\\u00e0m hai b\\u00ean .\", \"Khi chi\\u1ebfm_\\u0111o\\u1ea1t , S\\u01a1n kh\\u00f4ng c\\u00f2n \\u1edf OceanBank nh\\u01b0ng <person> S\\u01a1n </person> \\u0111\\u00e3 d\\u00f9ng t\\u00e1c_\\u0111\\u1ed9ng c\\u1ee7a c\\u1ed5_\\u0111\\u00f4ng l\\u1edbn chi_ph\\u1ed1i ho\\u1ea1t_\\u0111\\u1ed9ng v\\u1ed1n c\\u1ee7a <organization> PVN </organization> t\\u1ea1i OceanBank \\u0111\\u1ec3 chi\\u1ebfm_\\u0111o\\u1ea1t ti\\u1ec1n .\", \"V\\u1ec1 h\\u00ecnh_ph\\u1ea1t \\u0111\\u1ed1i_v\\u1edbi <person> Ho\\u00e0ng_Th\\u1ecb_H\\u1ed3ng_T\\u1ee9 </person> - nguy\\u00ean Ch\\u1ee7_t\\u1ecbch <organization> BSC </organization> , Vi\\u1ec7n KSND \\u0111\\u1ec1_ngh\\u1ecb H\\u0110XX ph\\u1ea1t 30 - 36 th\\u00e1ng t\\u00f9 nh\\u01b0ng cho h\\u01b0\\u1edfng\\u00a0\\u00e1n treo , th\\u1eddi_gian th\\u1eed_th\\u00e1ch 2 n\\u0103m .\", \"\\u0110\\u1ea1i_di\\u1ec7n VKS c\\u0169ng \\u0111\\u1ec1_ngh\\u1ecb \\u00e1p_d\\u1ee5ng th\\u00eam t\\u00ecnh_ti\\u1ebft gi\\u1ea3m nh\\u1eb9 cho c\\u00e1c b\\u1ecb_c\\u00e1o Ph\\u1ea1m_Ho\\u00e0ng_Giang ( c\\u1ef1u TG\\u0110 BSC ) , L\\u00ea_Th\\u1ecb_Thu_Thu\\u1ef7 ( c\\u1ef1u Ph\\u00f3 TG\\u0110 Oceanbank ) , V\\u0169_Th\\u1ecb_Thu\\u1ef3_D\\u01b0\\u01a1ng ( nguy\\u00ean Tr\\u01b0\\u1edfng ban k\\u1ebf_to\\u00e1n ) , H\\u1ee9a_Th\\u1ecb_Ph\\u1ea5n ( nguy\\u00ean Ch\\u1ee7_t\\u1ecbch H\\u0110QT c\\u00f4ng_ty Ph\\u00fa_M\\u1ef9 ) \\u00c1p_d\\u1ee5ng th\\u00eam h\\u00ecnh_ph\\u1ea1t l\\u00e0 \\u00e1n_treo \\u0111\\u1ed1i_v\\u1edbi <person> Ph\\u1ea1m_H\\u1ed3ng_T\\u1ee9 </person> ( nguy\\u00ean Ch\\u1ee7_t\\u1ecbch H\\u0110QT <organization> c\\u00f4ng_ty BSC </organization> ) , \\u0111\\u1ec1_ngh\\u1ecb x\\u1eed_ph\\u1ea1t b\\u1ecb_c\\u00e1o T\\u1ee9 m\\u1ee9c \\u00e1n_t\\u1eeb 30-36 th\\u00e1ng t\\u00f9_treo .\", \"N\\u1eef hung_th\\u1ee7 th\\u1eeba_nh\\u1eadn \\u0111\\u00e3 d\\u00f9ng ch\\u00e0y nh\\u00f4m s\\u00e1t_h\\u1ea1i Ch\\u1ee7_nhi\\u1ec7m H\\u1ee3p_t\\u00e1c_x\\u00e3 Sau nhi\\u1ec1u ng\\u00e0y \\u0111i\\u1ec1u_tra , C\\u00f4ng_an t\\u1ec9nh B\\u1eafc_Ninh \\u0111\\u00e3 c\\u00f3 \\u0111\\u1ee7 ch\\u1ee9ng_c\\u1ee9 kh\\u1eb3ng_\\u0111\\u1ecbnh Nguy\\u1ec5n_Th\\u1ecb_Th\\u1ea3o ( SN 1975 ) , tr\\u00fa t\\u1ea1i th\\u00f4n Du\\u1ec7_\\u0110\\u00f4ng , <location> th\\u1ecb_tr\\u1ea5n Lim </location> , <location> huy\\u1ec7n Ti\\u00ean_Du </location> , B\\u1eafc_Ninh l\\u00e0 k\\u1ebb ra_tay gi\\u1ebft ch\\u1ebft b\\u00e0 Hu\\u1ea5n .\", \"Ng\\u00e0y 22/9 , c\\u00f4ng_an huy\\u1ec7n Nghi_L\\u1ed9c , t\\u1ec9nh Ngh\\u1ec7_An cho bi\\u1ebft hi\\u1ec7n \\u0111\\u01a1n_v\\u1ecb \\u0111\\u00e3 ra quy\\u1ebft_\\u0111\\u1ecbnh kh\\u1edfi_t\\u1ed1 b\\u1ecb_can \\u0111\\u1ed1i_v\\u1edbi <person> L\\u00ea_V\\u0103n_Thi\\u1ebft </person> ( 40 tu\\u1ed5i , tr\\u00fa x\\u00e3 Nghi_Ki\\u1ec1u , <location> huy\\u1ec7n Nghi_L\\u1ed9c </location> ) v\\u1ec1 h\\u00e0nh_vi C\\u1ed1_\\u00fd g\\u00e2y th\\u01b0\\u01a1ng_t\\u00edch .\", \"\\u0110\\u1ed1i_t\\u01b0\\u1ee3ng v\\u1edd v\\u00e0o c\\u1eeda_h\\u00e0ng mua_b\\u00e1n , s\\u1eeda_ch\\u1eefa \\u0111i\\u1ec7n_tho\\u1ea1i c\\u0169 \\u1edf s\\u1ed1 34 <location> Quan_Nh\\u00e2n </location> ( ph\\u01b0\\u1eddng Trung_Ho\\u00e0 , <location> C\\u1ea7u_Gi\\u1ea5y </location> ) \\u0111\\u1ec3 xem v\\u00e0 c\\u1ea7m chi\\u1ebfc iPhone 6S Plus b\\u1ecf ch\\u1ea1y \\u0111\\u00e3 ra \\u0111\\u1ea7u_th\\u00fa t\\u1ea1i tr\\u1ee5_s\\u1edf c\\u00f4ng_an ph\\u01b0\\u1eddng v\\u00e0o ng\\u00e0y 22-9 .\", \"Tr\\u01b0\\u1edbc \\u0111\\u00f3 , V\\u0103n_ph\\u00f2ng Ch\\u00ednh_ph\\u1ee7 truy\\u1ec1n_\\u0111\\u1ea1t \\u00fd_ki\\u1ebfn Th\\u1ee7_t\\u01b0\\u1edbng Nguy\\u1ec5n_Xu\\u00e2n_Ph\\u00fac y\\u00eau_c\\u1ea7u UBND t\\u1ec9nh Qu\\u1ea3ng_Nam ch\\u1ec9_\\u0111\\u1ea1o c\\u00e1c c\\u01a1_quan_ch\\u1ee9c_n\\u0103ng x\\u00e1c_minh th\\u00f4ng_tin v\\u1ec1 vi\\u1ec7c ph\\u00e1 r\\u1eebng_ph\\u00f2ng_h\\u1ed9 t\\u1ea1i <location> x\\u00e3 Ti\\u00ean_L\\u00e3nh </location> , <location> huy\\u1ec7n Ti\\u00ean_Ph\\u01b0\\u1edbc </location> .\", \"Angelian_Jolie v\\u00e0 6 \\u0111\\u1ee9a con c\\u00f9ng c\\u00e1c c\\u1ed9ng_s\\u1ef1 gi\\u1edbi_thi\\u1ec7u phim t\\u1ea1i Li\\u00ean_hoan_phim Toronto_V\\u1edbi 6 \\u0111\\u1ee9a con - Maddox , 16 , <person> Pax </person> , 13 , Zahara , 12 , Shiloh , 11 , <person> Vivienne </person> and Knox , 9 - c\\u00f9ng \\u0111i , ng\\u00f4i_sao m\\u00e0n_b\\u1ea1c 42 tu\\u1ed5i n\\u