dc Ho\\u00e0ng_Hoa_Th\\u00e1m </organization> <location> ( Ph\\u01b0\\u1eddng V\\u0129nh_Ph\\u00fac </location> , Qu\\u1eadn Ba_\\u0110\\u00ecnh , H\\u00e0_N\\u1ed9i ) vinh_d\\u1ef1 \\u0111\\u01b0\\u1ee3c C\\u00f4ng_an TP.\", \"B\\u00e3o s\\u1ed1 10 l\\u00e0 c\\u01a1n b\\u00e3o m\\u1ea1nh \\u0111\\u1ed5_b\\u1ed9 v\\u00e0o khu_v\\u1ef1c <location> mi\\u1ec1n Trung </location> , tr\\u1ef1c_ti\\u1ebfp l\\u00e0 v\\u00e0o <location> H\\u00e0_T\\u0129nh </location> v\\u00e0 Qu\\u1ea3ng_B\\u00ecnh , l\\u00e0m nhi\\u1ec1u ng\\u01b0\\u1eddi ch\\u1ebft , m\\u1ea5t_t\\u00edch v\\u00e0 b\\u1ecb_th\\u01b0\\u01a1ng , g\\u00e2y thi\\u1ec7t_h\\u1ea1i v\\u1ec1 t\\u00e0i_s\\u1ea3n tr\\u00ean 11.000 t\\u1ef7 \\u0111\\u1ed3ng .\", \"N\\u1eef hung_th\\u1ee7 th\\u1eeba_nh\\u1eadn \\u0111\\u00e3 d\\u00f9ng ch\\u00e0y nh\\u00f4m s\\u00e1t_h\\u1ea1i Ch\\u1ee7_nhi\\u1ec7m H\\u1ee3p_t\\u00e1c_x\\u00e3 Sau nhi\\u1ec1u ng\\u00e0y \\u0111i\\u1ec1u_tra , C\\u00f4ng_an t\\u1ec9nh B\\u1eafc_Ninh \\u0111\\u00e3 c\\u00f3 \\u0111\\u1ee7 ch\\u1ee9ng_c\\u1ee9 kh\\u1eb3ng_\\u0111\\u1ecbnh Nguy\\u1ec5n_Th\\u1ecb_Th\\u1ea3o ( SN 1975 ) , tr\\u00fa t\\u1ea1i th\\u00f4n Du\\u1ec7_\\u0110\\u00f4ng , th\\u1ecb_tr\\u1ea5n Lim , <location> huy\\u1ec7n Ti\\u00ean_Du </location> , <location> B\\u1eafc_Ninh </location> l\\u00e0 k\\u1ebb ra_tay gi\\u1ebft ch\\u1ebft b\\u00e0 Hu\\u1ea5n .\", \"B\\u1ed9_tr\\u01b0\\u1edfng Th\\u1ec3_thao <location> Anh </location> <person> Tracey_Crouch </person> cho r\\u1eb1ng c\\u00e2u_chuy\\u1ec7n c\\u1ee7a Sampson cho th\\u1ea5y c\\u00f3 qu\\u00e1 nhi\\u1ec1u b\\u1ea5t_c\\u1eadp trong vi\\u1ec7c FA tuy\\u1ec3n_ch\\u1ecdn HLV cho c\\u00e1c \\u0111\\u1ed9i_tuy\\u1ec3n khi nh\\u00e2n_th\\u00e2n v\\u00e0 t\\u01b0_c\\u00e1ch \\u0111\\u1ea1o_\\u0111\\u1ee9c kh\\u00f4ng \\u0111\\u01b0\\u1ee3c xem tr\\u1ecdng .\", \"\\u0110\\u00e3 c\\u00f3 r\\u1ea5t nhi\\u1ec1u gi\\u00e1o_d\\u00e2n di_c\\u01b0 v\\u00e0o <location> Nam </location> trong th\\u1eddi_gian n\\u00e0y , trong \\u0111\\u00f3 bao_g\\u1ed3m c\\u1ea3 <person> V\\u0169_Ng\\u1ecdc_Nh\\u1ea1 </person> .\", \"M\\u1edbi_\\u0111\\u00e2y , HLV tr\\u01b0\\u1edfng c\\u1ee7a MU - \\u00f4ng Mourinho \\u0111\\u00e3 quy\\u1ebft_\\u0111\\u1ecbnh ra_m\\u1eb7t nh\\u1eb1m n\\u00edu ch\\u00e2n ti\\u1ec1n_v\\u1ec7 ng\\u01b0\\u1eddi B\\u1ec9 - <person> Marouane_Fellaini </person> ti\\u1ebfp_t\\u1ee5c g\\u1eafn_b\\u00f3 v\\u1edbi <organization> MU </organization> .\", \"T\\u1eeb \\u0111\\u00f3 Long \\u0111\\u00e3 n\\u1ea3y_sinh \\u00fd_\\u0111\\u1ecbnh gi\\u1ebft anh <person> Nguy\\u1ec5n_V\\u0103n_K\\u1ef7 </person> ( SN 1970 ) ch\\u1ed3ng ch\\u1ecb <person> L\\u1ec7 </person> \\u0111\\u1ec3 \\u0111\\u01b0\\u1ee3c t\\u1ef1_do \\u0111\\u1ebfn v\\u1edbi ch\\u1ecb L\\u1ec7 .\", \"T\\u1ed5ng gi\\u00e1_tr\\u1ecb g\\u1ea7n 3,4 t\\u1ef7 \\u0111\\u1ed3ng .\", \"Sau khi ly_h\\u00f4n v\\u1edbi <person> Brad_Pitt </person> v\\u00e0o ng\\u00e0y 19/9 n\\u0103m_ngo\\u00e1i , <person> Jolie </person> g\\u1ea7n nh\\u01b0 ho\\u00e0n_to\\u00e0n r\\u00fat kh\\u1ecfi \\u00e1nh \\u0111\\u00e8n s\\u00e2n_kh\\u1ea5u \\u0111\\u1ec3 ch\\u00fa_t\\u00e2m ch\\u0103m_s\\u00f3c gia_\\u0111\\u00ecnh , gi\\u00fap c\\u00e1c con h\\u00e0n_g\\u1eafn n\\u1ed7i \\u0111au do \\u0111\\u1ed5_v\\u1ee1 h\\u00f4n_nh\\u00e2n c\\u1ee7a cha_m\\u1eb9 .\", \"Khi chi\\u1ebfm_\\u0111o\\u1ea1t , S\\u01a1n kh\\u00f4ng c\\u00f2n \\u1edf OceanBank nh\\u01b0ng <person> S\\u01a1n </person> \\u0111\\u00e3 d\\u00f9ng t\\u00e1c_\\u0111\\u1ed9ng c\\u1ee7a c\\u1ed5_\\u0111\\u00f4ng l\\u1edbn chi_ph\\u1ed1i ho\\u1ea1t_\\u0111\\u1ed9ng v\\u1ed1n c\\u1ee7a PVN t\\u1ea1i <organization> OceanBank </organization> \\u0111\\u1ec3 chi\\u1ebfm_\\u0111o\\u1ea1t ti\\u1ec1n .\", \"<location> M\\u1ef9 </location> <organization> - SDF </organization> \\u0111\\u00e3 ph\\u1ea3n_\\u1ee9ng quy\\u1ebft_li\\u1ec7t .\", \"Trong th\\u1eddi_gian n\\u00e0y , Thanh l\\u1ea5y t\\u00ean con g\\u00e1i \\u0111\\u1ec3 th\\u00e0nh_l\\u1eadp <organization> C\\u00f4ng_ty TNHH Kh\\u00e1nh_Linh </organization> ( tr\\u1ee5_s\\u1edf t\\u1ea1i <location> Ninh_B\\u00ecnh </location> ) ti\\u1ebfp_t\\u1ee5c gi\\u1edf chi\\u00eau l\\u1eeba_\\u0111\\u1ea3o .\", \"Tr\\u01b0\\u1edbc \\u0111\\u00f3 v\\u00e0o chi\\u1ec1u_t\\u1ed1i nay , t\\u1ea1i cu\\u1ed9c l\\u00e0m_vi\\u1ec7c v\\u1edbi c\\u01a1_quan_ch\\u1ee9c_n\\u0103ng sau khi ki\\u1ec3m_tra th\\u1ef1c_\\u0111\\u1ecba v\\u1ee5 ph\\u00e1 r\\u1eebng_ph\\u00f2ng_h\\u1ed9 t\\u1ea1i x\\u00e3 Ti\\u00ean_L\\u00e3nh , <location> huy\\u1ec7n Ti\\u00ean_Ph\\u01b0\\u1edbc </location> , Qu\\u1ea3ng_Nam , Ph\\u00f3 ch\\u1ee7_t\\u1ecbch t\\u1ec9nh <person> L\\u00ea_Tr\\u00ed_Thanh </person> y\\u00eau_c\\u1ea7u c\\u01a1_quan ki\\u1ec3m_l\\u00e2m v\\u00e0 c\\u00f4ng_an t\\u1ec9nh kh\\u1edfi_t\\u1ed1 v\\u1ee5 \\u00e1n \\u0111\\u1ec3 \\u0111i\\u1ec1u_tra , l\\u00e0m r\\u00f5 .\", \"Trong s\\u1ed1 \\u0111\\u00f3 , ri\\u00eang <location> huy\\u1ec7n Ho\\u1eb1ng_Ho\\u00e1 </location> <location> ( Thanh_Ho\\u00e1 </location> ) , m\\u1ed9t huy\\u1ec7n v\\u00f9ng ven bi\\u1ec3n c\\u0169ng \\u0111\\u01b0\\u1ee3c cho l\\u00e0 ch\\u1ecbu s\\u1ee9c t\\u00e0n_ph\\u00e1 n\\u1eb7ng_n\\u1ec1 c\\u1ee7a c\\u01a1n b\\u00e3o s\\u1ed1 10 , d\\u00f9 n\\u1eb1m ngo\\u00e0i r\\u00eca c\\u1ee7a t\\u00e2m_b\\u00e3o .\", \"B\\u1ed9_tr\\u01b0\\u1edfng T\\u00f4_L\\u00e2m ti\\u1ebfp \\u0111\\u1ea1i_s\\u1ee9 \\u0111\\u1eb7c_m\\u1ec7nh to\\u00e0n_quy\\u1ec1n CH Th\\u1ed5_Nh\\u0129_K\\u1ef3 Chi\\u1ec1u 22-9 , Th\\u01b0\\u1ee3ng_t\\u01b0\\u1edbng T\\u00f4_L\\u00e2m , B\\u1ed9_tr\\u01b0\\u1edfng B\\u1ed9 C\\u00f4ng_an \\u0111\\u00e3 c\\u00f3 bu\\u1ed5i ti\\u1ebfp ng\\u00e0i <person> Akif_Ayhan </person> , \\u0110\\u1ea1i_s\\u1ee9 \\u0111\\u1eb7c_m\\u1ec7nh to\\u00e0n_quy\\u1ec1n C\\u1ed9ng_ho\\u00e0 Th\\u1ed5_Nh\\u0129_K\\u1ef3 t\\u1ea1i <location> Vi\\u1ec7t_Nam </location> .\", \"Chi\\u1ec1u c\\u00f9ng ng\\u00e0y , l\\u1ef1c_l\\u01b0\\u1ee3ng c\\u00f4ng_an b\\u1eaft_gi\\u1eef 3 \\u0111\\u1ed1i_t\\u01b0\\u1ee3ng l\\u00e0 Ph\\u1ea1m_V\\u0103n_Hi\\u1ec7p , <person> H\\u00e0_V\\u0103n_Th\\u01b0\\u01a1ng </person> , H\\u00e0_V\\u0103n_Thu\\u1ea7n ( c\\u00f9ng tr\\u00fa t\\u1ea1i <location> th\\u00f4n \\u0110\\u1ed3ng L\\u1ee5c </location> , x\\u00e3 Gia_Ph\\u00fa ) .\", \"Trong th\\u1eddi_k\\u1ef3 th\\u1ebf_gi\\u1edbi tr\\u1ea3i qua nhi\\u1ec1u b\\u1ea5t_\\u1ed5n v\\u1edbi vi\\u1ec7c n\\u01b0\\u1edbc M\\u1ef9 do \\u00f4ng Donald_Trump l\\u00e3nh_\\u0111\\u1ea1o , v\\u1ea5n_\\u0111\\u1ec1 Brexit <location> ( Anh </location> r\\u1eddi Li\\u00ean_minh ch\\u00e2u_\\u00c2u - <organization> EU </organization> ) , c\\u00f9ng v\\u1edbi c\\u00e1c cu\\u1ed9c kh\\u1ee7ng_ho\\u1ea3ng to\\u00e0n_c\\u1ea7u , n\\u1eef l\\u00e3nh_\\u0111\\u1ea1o 63 tu\\u1ed5i n\\u00e0y \\u0111\\u00e3 tr\\u1edf_th\\u00e0nh ch\\u1ed7 d\\u1ef1a v\\u1eefng_ch\\u1eafc cho m\\u1ed9t \\u0111\\u1ea5t_n\\u01b0\\u1edbc d\\u00e0nh nhi\\u1ec1u quan_t\\u00e2m v\\u1ec1 v\\u1ea5n_\\u0111\\u1ec1 duy_tr\\u00ec t\\u1ed1c_\\u0111\\u1ed9 t\\u0103ng_tr\\u01b0\\u1edfng v\\u00e0 kh\\u1ea3_n\\u0103ng t\\u1ea1o c\\u00f4ng_\\u0103n_vi\\u1ec7c_l\\u00e0m mong_mu\\u1ed1n cho ng\\u01b0\\u1eddi_d\\u00e2n .\", \"L\\u00e3nh_\\u0111\\u1ea1o Kim Jong-un v\\u1eaby tay ch\\u00e0o \\u0111\\u00e1m \\u0111\\u00f4ng trong chuy\\u1ebfn th\\u1ecb_s\\u00e1t m\\u1ed9t ng\\u00f4i l\\u00e0ng m\\u1edbi \\u0111\\u01b0\\u1ee3c x\\u00e2y_d\\u1ef1ng \\u1edf <location> Paekhak-dong </location> , <location> Sonbong </location> , th\\u00e0nh_ph\\u1ed1 Rason .\", \"Trong s\\u1ed1 51 b\\u1ecb_c\\u00e1o , VKS \\u0111\\u1ec1_ngh\\u1ecb mi\\u1ec5n h\\u00ecnh_ph\\u1ea1t v\\u1edbi 4 ng\\u01b0\\u1eddi : Nguy\\u1ec5n_Vi\\u1ec7t_H\\u00e0 ( nguy\\u00ean Gi\\u00e1m_\\u0111\\u1ed1c Oceanbank chi_nh\\u00e1nh Th\\u00e1i_B\\u00ecnh ) , Nguy\\u1ec5n_Phan_Trung_Ki\\u00ean ( nguy\\u00ean Gi\\u00e1m_\\u0111\\u1ed1c Oceanbank_Ph\\u00f2ng giao_d\\u1ecbch \\u0110\\u00f4ng_\\u0110\\u00f4 ) , <person> Nguy\\u1ec5n_Th\\u1ecb_Loan </person> ( nguy\\u00ean Gi\\u00e1m_\\u0111\\u1ed1c Oceanbank <organization> Ph\\u00f2ng giao_d\\u1ecbch Trung_Y\\u00ean </organization> ) v\\u00e0 Tr\\u1ea7n_Anh_Thi\\u1ebft ( nguy\\u00ean Gi\\u00e1m_\\u0111\\u1ed1c Oceanbank chi_nh\\u00e1nh H\\u00e0_N\\u1ed9i ) .\", \"V\\u1edbi \\u00f4ng <person> Nguy\\u1ec5n_Xu\\u00e2n_Anh </person> , U\\u1ef7_vi\\u00ean Trung_\\u01b0\\u01a1ng \\u0110\\u1ea3ng , B\\u00ed_th\\u01b0 Th\\u00e0nh_u\\u1ef7 , Ch\\u1ee7_t\\u1ecbch <organization> H\\u0110ND TP \\u0110\\u00e0_N\\u1eb5ng </organization> c\\u0169ng c\\u00f3 c\\u00e1c vi_ph\\u1ea1m , khuy\\u1ebft_\\u0111i\\u1ec3m .\", \"N\\u1eef hung_th\\u1ee7 th\\u1eeba_nh\\u1eadn \\u0111\\u00e3 d\\u00f9ng ch\\u00e0y nh\\u00f4m s\\u00e1t_h\\u1ea1i Ch\\u1ee7_nhi\\u1ec7m H\\u1ee3p_t\\u00e1c_x\\u00e3 Sau nhi\\u1ec1u ng\\u00e0y \\u0111i\\u1ec1u_tra , C\\u00f4ng_an t\\u1ec9nh B\\u1eafc_Ninh \\u0111\\u00e3 c\\u00f3 \\u0111\\u1ee7 ch\\u1ee9ng_c\\u1ee9 kh\\u1eb3ng_\\u0111\\u1ecbnh Nguy\\u1ec5n_Th\\u1ecb_Th\\u1ea3o ( SN 1975 ) , tr\\u00fa t\\u1ea1i <location> th\\u00f4n Du\\u1ec7_\\u0110\\u00f4ng </location> , th\\u1ecb_tr\\u1ea5n Lim , huy\\u1ec7n Ti\\u00ean_Du , <location> B\\u1eafc_Ninh </location> l\\u00e0 k\\u1ebb ra_tay gi\\u1ebft ch\\u1ebft b\\u00e0 Hu\\u1ea5n .\", \"Theo b\\u1ea3n_\\u00e1n s\\u01a1_th\\u1ea9m , kho\\u1ea3ng th\\u00e1ng 7/2015 , <person> \\u0110o\\u00e0n </person> v\\u00e0 em <person> Tr\\u1ea7n_Th\\u1ecb_Tr\\u00fac L. </person>\", \"Theo \\u00e1n s\\u01a1_th\\u1ea9m , t\\u1ed1i 20-3-2015 , Th\\u00e0nh \\u0111\\u1ebfn nh\\u00e0 anh L\\u00ea_Minh_T\\u00e2m <location> ( ph\\u01b0\\u1eddng \\u0110\\u00f4ng_H\\u01b0ng_Thu\\u1eadn </location> , <location> qu\\u1eadn 12 </location> , TP HCM ) \\u0111\\u1ec3 d\\u1ef1 \\u0111\\u00e1m_tang .\", \"Thay_m\\u1eb7t gia_\\u0111\\u00ecnh , t\\u00f4i xin \\u0111\\u01b0\\u1ee3c nh\\u1eafc l\\u1ea1i to\\u00e0n_b\\u1ed9 s\\u1ef1 cam_k\\u1ebft v\\u00e0 l\\u00f2ng trung_th\\u00e0nh c\\u1ee7a ch\\u00fang_t\\u00f4i v\\u1edbi <organization> L \\u2019 Oreal </organization> , v\\u00e0 m\\u1ed9t l\\u1ea7n n\\u1eefa b\\u00e0y_t\\u1ecf s\\u1ef1 tin_t\\u01b0\\u1edfng c\\u1ee7a t\\u00f4i v\\u00e0o Ch\\u1ee7_t\\u1ecbch c\\u00f4ng_ty , \\u00f4ng <person> Jean-Paul Agon </person> , v\\u00e0 \\u0111\\u1ed9i_ng\\u0169 c\\u1ee7a \\u00f4ng \\u1ea5y tr\\u00ean to\\u00e0n th\\u1ebf_gi\\u1edbi \\u201d .\", \"VKS \\u0111\\u1ec1_ngh\\u1ecb gi\\u1ea3m m\\u1ed9t ph\\u1ea7n h\\u00ecnh_ph\\u1ea1t cho nh\\u00f3m b\\u1ecb_c\\u00e1o gi\\u00e1m_\\u0111\\u1ed1c chi_nh\\u00e1nh Oceanbank c\\u00f3 m\\u1ee9c \\u00e1n \\u0111\\u1ec1_ngh\\u1ecb 36-42 th\\u00e1ng t\\u00f9 g\\u1ed3m : L\\u00ea_Tu\\u1ea5n_Anh ( Gi\\u00e1m_\\u0111\\u1ed1c chi_nh\\u00e1nh Th\\u0103ng_Long ) , Nguy\\u1ec5n_Th\\u1ecb_Ki\\u1ec1u_Li\\u00ean ( Gi\\u00e1m_\\u0111\\u1ed1c chi_nh\\u00e1nh V\\u0169ng_T\\u00e0u ) , <person> Nguy\\u1ec5n_Minh_\\u0110\\u1ea1o </person> ( Gi\\u00e1m_\\u0111\\u1ed1c chi_nh\\u00e1nh <location> H\\u00e0_N\\u1ed9i </location> ) , Ho\\u00e0ng_B\\u00edch_V\\u00e2n ( chi_nh\\u00e1nh TP HCM ) , Nguy\\u1ec5n_Qu\\u1ed1c_Chi\\u1ebfn ( S\\u00e0i_G\\u00f2n ) , Tr\\u1ea7n_Th\\u1ecb_Thu_H\\u01b0\\u01a1ng ( H\\u1ea3i_D\\u01b0\\u01a1ng ) .\", \"<organization> SDF </organization> \\u0111\\u00e3 th\\u00e0nh_l\\u1eadp c\\u1ea5p_t\\u1ed1c <organization> \\u201c U\\u1ef7_ban ti\\u1ebfp_qu\\u1ea3n Deir_Ezzor </organization> \\u201d v\\u00e0 c\\u00f9ng c\\u00e1c tuy\\u00ean_b\\u1ed1 g\\u00e2y s\\u1ed1c nh\\u01b0 \\u201c kh\\u00f4ng cho_ph\\u00e9p SAA v\\u01b0\\u1ee3t s\\u00f4ng Euphrates \\u201d \\u2026 2 , \\u0110\\u1ed9i \\u0111\\u1eb7c_nhi\\u1ec7m SAA c\\u00f9ng c\\u1ed1_v\\u1ea5n Nga d\\u01b0\\u1edbi s\\u1ef1 h\\u1ed7_tr\\u1ee3 c\\u1ee7a VKS Nga \\u0111\\u00e3 v\\u01b0\\u1ee3t s\\u00f4ng Euphrates \\u0111\\u00e1nh chi\\u1ebfm \\u0111\\u1ea7u c\\u1ea7u th\\u00e0nh_c\\u00f4ng .\", \"Tuy_nhi\\u00ean , \\u201c hi\\u1ec7u_\\u1ee9ng Martin_Schulz \\u201d c\\u0169ng d\\u1ea7n qua \\u0111i khi <organization> SPD </organization> kh\\u00f4ng \\u0111\\u01b0a ra \\u0111\\u01b0\\u1ee3c c\\u01b0\\u01a1ng_l\\u0129nh tranh_c\\u1eed \\u0111\\u1ee7 s\\u1ee9c thuy\\u1ebft_ph\\u1ee5c khi\\u1ebfn c\\u1eed_tri \\u0110\\u1ee9c c\\u00f3_th\\u1ec3 thay_\\u0111\\u1ed5i , c\\u00f2n <organization> CDU / CSU </organization> nh\\u00ecn_chung v\\u1eabn \\u0111ang \\u0111\\u1ea3m_\\u0111\\u01b0\\u01a1ng t\\u1ed1t nhi\\u1ec7m_v\\u1ee5 l\\u00e3nh_\\u0111\\u1ea1o \\u0111\\u1ea5t_n\\u01b0\\u1edbc .\", \"HCM ) : Ch\\u1ee3 t\\u1ef1_ph\\u00e1t - lo_ng\\u1ea1i v\\u1ec7_sinh an_to\\u00e0n th\\u1ef1c_ph\\u1ea9m N\\u1eb1m c\\u1ea1nh ch\\u1ee3 hoa H\\u1ed3_Th\\u1ecb_K\\u1ef7 , \\u0111i\\u1ec3m ch\\u1ee3 t\\u1ef1_ph\\u00e1t L\\u00ea_H\\u1ed3ng_Phong <location> ( h\\u1ebbm 438 </location> , L\\u00ea_H\\u1ed3ng_Phong , <location> ph\\u01b0\\u1eddng 1 </location> , qu\\u1eadn 10 ) \\u0111\\u00e3 c\\u00f3 ho\\u1ea1t_\\u0111\\u1ed9ng bu\\u00f4n_b\\u00e1n nhi\\u1ec1u n\\u0103m qua .\", \"\\u0110\\u01b0\\u1ee3c bi\\u1ebft , \\u1edf Vi\\u1ec7t_Nam , c\\u00e1_\\u0111u\\u1ed1i n\\u01b0\\u1edbc_ng\\u1ecdt \\\" kh\\u1ee7ng \\\" nh\\u1ea5t t\\u1eebng xu\\u1ea5t_hi\\u1ec7n ch\\u1ec9 c\\u00f3 c\\u00e2n n\\u1eb7ng kho\\u1ea3ng 163 kg , do m\\u1ed9t ng\\u01b0_d\\u00e2n \\u1ea5p T\\u00e2n_Ho\\u00e0_B , x\\u00e3 T\\u00e2n_An , <location> th\\u1ecb_x\\u00e3 T\\u00e2n_Ch\\u00e2u </location> , <location> An_Giang </location> b\\u1eaft \\u0111\\u01b0\\u1ee3c trong l\\u00fac k\\u00e9o ghe c\\u00e0o tr\\u00ean s\\u00f4ng Ti\\u1ec1n .\", \"VKS \\u0111\\u1ec1_ngh\\u1ecb mi\\u1ec5n h\\u00ecnh_ph\\u1ea1t v\\u1edbi c\\u00e1c b\\u1ecb_c\\u00e1o : Nguy\\u1ec5n_Vi\\u1ec7t_H\\u00e0 ( 40 tu\\u1ed5i , gi\\u00e1m_\\u0111\\u1ed1c Ph\\u00f2ng giao_d\\u1ecbch \\u0110\\u00e0o_Duy_Anh , H\\u00e0_N\\u1ed9i ) ; Nguy\\u1ec5n_Phan_Trung_Ki\\u00ean ( 35 tu\\u1ed5i , nguy\\u00ean gi\\u00e1m_\\u0111\\u1ed1c chi_nh\\u00e1nh Ph\\u00f2ng giao_d\\u1ecbch \\u0110\\u00f4ng_\\u0110\\u00f4 , H\\u00e0_N\\u1ed9i ) ; <person> Nguy\\u1ec5n_Th\\u1ecb_Loan </person> ( 38 tu\\u1ed5i , nguy\\u00ean gi\\u00e1m_\\u0111\\u1ed1c chi_nh\\u00e1nh <organization> Ph\\u00f2ng giao_d\\u1ecbch Trung_Y\\u00ean </organization> ( H\\u00e0_N\\u1ed9i ) , Tr\\u1ea7n_V\\u0